<?xml version="1.0" encoding="iso-8859-1"?>
<!-- This is an application-description file of the kind read by the CL-CEC.
	   It has one, dummy application, and is used as default until the operator 
	   configures a proper description-file. It's not sufficient to run the
	   CL-CEC in service but saves the CEC local controls from crashing due to
	   uninitialized variables during the set-up process. -->
<CommandLineExecutionControllerConfig xmlns="http://www.astrogrid.org/schema/CEAImplementation/v1"
	xmlns:ceab="http://www.astrogrid.org/schema/CommonExecutionArchitectureBase/v1"
	xmlns:agpd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.astrogrid.org/schema/CEAImplementation/v1
	                    http://software.astrogrid.org/schema/cea/CEAImplementation/v1.0/CEAImplementation.xsd">
	<!-- The application name is its IVO identifier.
		   The org.astrogrid.localhost is the authority ID; Dummy is the resource key. -->
	<Application name="ivo://org.astrogrid.localhost/Dummy">
		<ceab:Parameters/>
		<ceab:Interfaces>
			<ceab:Interface name="Dummy">
				<ceab:input/>
				<ceab:output/>
			</ceab:Interface>
		</ceab:Interfaces>
		<ExecutionPath>/nix/nada/not/there</ExecutionPath>
		<LongName>Dummy</LongName>
	</Application>
</CommandLineExecutionControllerConfig>
