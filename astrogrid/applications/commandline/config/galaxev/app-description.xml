        <UI_Name>Flux axis units</UI_Name>
        <UI_Description>Flux axis of output is Flambda in Msun/Angstrom or Fnu in Msun/Hz</UI_Description>
        <DefaultValue>perAA</DefaultValue>
        <OptionList>
          <OptionVal>perAA</OptionVal>
          <OptionVal>perHz</OptionVal>
        </OptionList>
