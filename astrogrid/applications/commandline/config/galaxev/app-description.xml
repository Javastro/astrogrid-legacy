<?xml version="1.0"?>
<CommandLineExecutionControllerConfig xmlns="http://www.astrogrid.org/schema/CEAImplementation/v1"
    xmlns:ceai="http://www.astrogrid.org/schema/CEAImplementation/v1"
    xmlns:ceab="http://www.astrogrid.org/schema/CommonExecutionArchitectureBase/v1"
    xmlns:agpd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.astrogrid.org/schema/CEAImplementation/v1
	                http://software.astrogrid.org/schema/cea/CEAImplementation/v1.0/CEAImplementation.xsd">
  <!-- The application name is its IVO-ID.
       The org.astrogrid.localhost part is the authority ID;
       and sleep is the resource key. -->
  <Application name="org.astrogrid/Galaxev" version="2003">
    <ceab:Parameters>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="IMF" type="text" fileRef="false" switchType="keyword" commandSwitch="IMF">
        <UI_Name>IMF</UI_Name>
        <UI_Description>Initial Mass Function for model</UI_Description>
        <DefaultValue>salpeter</DefaultValue>
        <OptionList>
          <OptionVal>salpeter</OptionVal>
          <OptionVal>chabrier</OptionVal>
        </OptionList>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="RESOLUTION" type="text" fileRef="false" switchType="keyword"
        commandSwitch="RESOLUTION">
        <UI_Name>Resolution</UI_Name>
        <UI_Description>Low (lr) or High (hr) spectral resolution of output</UI_Description>
        <DefaultValue>lr</DefaultValue>
        <OptionList>
          <OptionVal>lr</OptionVal>
          <OptionVal>hr</OptionVal>
        </OptionList>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="TRACKS" type="text" fileRef="false" switchType="keyword" commandSwitch="TRACKS">
        <UI_Name>Track model</UI_Name>
        <UI_Description>Model used for stellar population evolutionary tracks. Please also choose
          appropriate metallicity</UI_Description>
        <DefaultValue>Padova1994</DefaultValue>
        <OptionList>
          <OptionVal>Padova1994</OptionVal>
          <OptionVal>Padova2000</OptionVal>
        </OptionList>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="Padova1994Metallicity" type="text" fileRef="false" switchType="keyword"
        commandSwitch="METALLICITY">
        <UI_Name>Padova1994 metallicity</UI_Name>
        <UI_Description>Metallicity for Padova1994 tracks. 0.02 is solar</UI_Description>
        <DefaultValue>0.02</DefaultValue>
        <OptionList>
          <OptionVal>0.0001</OptionVal>
          <OptionVal>0.0004</OptionVal>
          <OptionVal>0.004</OptionVal>
          <OptionVal>0.008</OptionVal>
          <OptionVal>0.02</OptionVal>
          <OptionVal>0.05</OptionVal>
        </OptionList>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="Padova2000Metallicity" type="text" fileRef="false" switchType="keyword"
        commandSwitch="METALLICITY">
        <UI_Name>Padova2000 metallicity</UI_Name>
        <UI_Description>Metallicity for Padova2000 tracks. 0.019 is solar</UI_Description>
        <DefaultValue>0.019</DefaultValue>
        <OptionList>
          <OptionVal>0.0004</OptionVal>
          <OptionVal>0.001</OptionVal>
          <OptionVal>0.004</OptionVal>
          <OptionVal>0.008</OptionVal>
          <OptionVal>0.019</OptionVal>
          <OptionVal>0.03</OptionVal>
        </OptionList>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="DUST" type="text" fileRef="false" switchType="keyword" commandSwitch="DUST">
        <UI_Name>Dust?</UI_Name>
        <UI_Description>Include dust in models? If Yes please enter optical depth parameters</UI_Description>
        <DefaultValue>N</DefaultValue>
        <OptionList>
          <OptionVal>N</OptionVal>
          <OptionVal>Y</OptionVal>
        </OptionList>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="TAU_V" type="text" fileRef="false" switchType="keyword" commandSwitch="TAU_V">
        <UI_Name>Dust tau_v</UI_Name>
        <UI_Description>Total effective attenuation optical depth</UI_Description>
        <DefaultValue>1.0</DefaultValue>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="MU" type="text" fileRef="false" switchType="keyword" commandSwitch="MU">
        <UI_Name>Dust mu</UI_Name>
        <UI_Description>Fraction of tau_v arising from the ambient ISM</UI_Description>
        <DefaultValue>0.3</DefaultValue>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="SFRTYPE" type="text" fileRef="false" switchType="keyword" commandSwitch="SFRTYPE">
        <UI_Name>SFR type</UI_Name>
        <UI_Description>Star formation rate model. SSP is one instantaneous burst; for other options
          please see descriptions for parameter selections below</UI_Description>
        <DefaultValue>exponential</DefaultValue>
        <OptionList>
          <OptionVal>SSP</OptionVal>
          <OptionVal>exponential</OptionVal>
          <OptionVal>singleburst</OptionVal>
          <OptionVal>constant</OptionVal>
          <OptionVal>delayed</OptionVal>
          <OptionVal>linear_decr</OptionVal>
        </OptionList>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="exponential_TAU" type="text" fileRef="false" switchType="keyword" commandSwitch="TAU">
        <UI_Name>Exponential SFR Tau</UI_Name>
        <UI_Description>e-folding timescale of exponentially declining star formation</UI_Description>
        <DefaultValue>1.0</DefaultValue>
        <Units>Gyr</Units>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="exponential_gas_recycle" type="text" fileRef="false" switchType="keyword"
        commandSwitch="GAS_RECYCLE">
        <UI_Name>Exponential SFR gas recycle?</UI_Name>
        <UI_Description>Recycle gas ejected by stars in exponentially declining star formation? If
          Yes please also enter epsilon</UI_Description>
        <DefaultValue>N</DefaultValue>
        <OptionList>
          <OptionVal>N</OptionVal>
          <OptionVal>Y</OptionVal>
        </OptionList>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="exponential_epsilon" type="text" fileRef="false" switchType="keyword"
        commandSwitch="EPSILON">
        <UI_Name>Exponential SFR gas recycle epsilon</UI_Name>
        <UI_Description>Fraction of gas recycled: epsilon ]0,1]. &lt;0 implies wind; &gt;1
          implies infall.</UI_Description>
        <DefaultValue>0.5</DefaultValue>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="exponential_tcut" type="text" fileRef="false" switchType="keyword"
        commandSwitch="TCUT">
        <UI_Name>Exponential SFR Tcut</UI_Name>
        <UI_Description>Set SFR back to zero at time Tcut</UI_Description>
        <DefaultValue>20</DefaultValue>
        <Units>Gyr</Units>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="singleburst_TAU" type="text" fileRef="false" switchType="keyword" commandSwitch="TAU">
        <UI_Name>singleburst SFR tau</UI_Name>
        <UI_Description>Single burst of star formation, duration tau</UI_Description>
        <DefaultValue>0.05</DefaultValue>
        <Units>Gyr</Units>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="constant_SFR" type="text" fileRef="false" switchType="keyword" commandSwitch="SFR">
        <UI_Name>Constant SFR </UI_Name>
        <UI_Description>Star formation rate for constant SFR</UI_Description>
        <DefaultValue>1.0</DefaultValue>
        <Units>Msun/yr</Units>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="constant_TCUT" type="boolean" fileRef="false" switchType="keyword"
        commandSwitch="TCUT">
        <UI_Name>Constant SFR Tcut</UI_Name>
        <UI_Description>Set SFR back to zero at time Tcut</UI_Description>
        <DefaultValue>20</DefaultValue>
        <Units>Gyr</Units>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="delayed_TAU" type="text" fileRef="false" switchType="keyword" commandSwitch="TAU">
        <UI_Name>Delayed SFR tau</UI_Name>
        <UI_Description>SFR reaches maximum at time tau</UI_Description>
        <DefaultValue>1</DefaultValue>
        <Units>Gyr</Units>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="delayed_TCUT" type="boolean" fileRef="false" switchType="keyword" commandSwitch="TCUT">
        <UI_Name>Delayed SFR Tcut</UI_Name>
        <UI_Description>Set SFR back to zero at time Tcut</UI_Description>
        <DefaultValue>20</DefaultValue>
        <Units>Gyr</Units>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="linear_decr_TAU" type="real" fileRef="false" switchType="keyword" commandSwitch="TAU">
        <UI_Name>Linearly decreasing SFR tau</UI_Name>
        <UI_Description>SFR is 0 at time tau</UI_Description>
        <DefaultValue>1</DefaultValue>
        <Units>Gyr</Units>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="linear_decr_TCUT" type="real" fileRef="false" switchType="keyword"
        commandSwitch="TCUT">
        <UI_Name>Linearly decreasing SFR Tcut</UI_Name>
        <UI_Description>Set SFR back to zero at time Tcut</UI_Description>
        <DefaultValue>20</DefaultValue>
        <Units>Gyr</Units>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="W1" type="real" fileRef="false" switchType="keyword" commandSwitch="W1">
        <UI_Name>Lower wavelength W1</UI_Name>
        <UI_Description>Lower limit to wavelength range of output spectra W0 [91,1.600E+06]</UI_Description>
        <DefaultValue>100</DefaultValue>
        <Units>Angstrom</Units>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="W2" type="real" fileRef="false" switchType="keyword" commandSwitch="W2">
        <UI_Name>Upper wavelength W2</UI_Name>
        <UI_Description>Upper limit to wavelength range of output spectra W0 [91,1.600E+06]</UI_Description>
        <DefaultValue>1.2E+06</DefaultValue>
        <Units>Angstrom</Units>
