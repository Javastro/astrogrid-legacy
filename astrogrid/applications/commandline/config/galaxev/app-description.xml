      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="WAVE_F0" type="text" fileRef="false" switchType="keyword" commandSwitch="F0">
        <UI_Name>WAVE_F0</UI_Name>
        <UI_Description>Normalise spectrum to luminosity F0 (at wavelength W0)</UI_Description>
        <DefaultValue>1</DefaultValue>
        <Units>Chosen unit (Msun perAA or perHz)</Units>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="WAVE_W0" type="text" fileRef="false" switchType="keyword" commandSwitch="W0">
        <UI_Name>WAVE_W0</UI_Name>
        <UI_Description>Normalise spectrum (to luminosity F0) at wavelength W0</UI_Description>
        <DefaultValue>5000</DefaultValue>
        <Units>Angstrom</Units>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="Fz_F0" type="text" fileRef="false" switchType="keyword" commandSwitch="F0">
        <UI_Name>Fz_F0</UI_Name>
        <UI_Description>Normalise spectrum to luminosity F0 (for a source at redshift z in FILTER)</UI_Description>
        <DefaultValue>1</DefaultValue>
        <Units>Chosen unit (Msun perAA or perHz)</Units>
      </ceai:CmdLineParameterDefn>
      <ceai:CmdLineParameterDefn xmlns="http://www.astrogrid.org/schema/AGParameterDefinition/v1"
        name="Fz_z" type="text" fileRef="false" switchType="keyword" commandSwitch="z">
        <UI_Name>Fz_z</UI_Name>
        <UI_Description>Normalise spectrum (to luminosity F0) for a source at redshift z (in FILTER)</UI_Description>
        <DefaultValue>1</DefaultValue>
      </ceai:CmdLineParameterDefn>
