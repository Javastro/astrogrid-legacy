	<!-- put the real applications here - note that they are not autodeployed, so the paths are fixed strings - this is more of a registry test....-->
	<Application name="REGAUTHORITY.SExtractor" instanceClass="org.astrogrid.applications.commandline.sextractor.SExtractor">
		<!--This is the application to concentrate on for the end of this iteration-->
		<Parameters>
			<!-- temporarily turn the images into direct file references - for performance during the AVO demo -->
			<CmdLineParameterDefn name="DetectionImage" type="binary" commandPosition="1" fileRef="true">
				<agpd:UI_Name>Detection Image</agpd:UI_Name>
				<agpd:UI_Description>The image that is used to detect sources. Basic position, shape and size informtaion is derived from this image</agpd:UI_Description>
			</CmdLineParameterDefn>
			<!--This is set to position 2 although it is actually merged with the position 1 image -->
			<CmdLineParameterDefn name="PhotoImage" type="binary" commandPosition="2" fileRef="true">
				<agpd:UI_Name>Measurement Image</agpd:UI_Name>
				<agpd:UI_Description>The Image that is used to measure photometric parameters</agpd:UI_Description>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="config_file" type="binary" commandSwitch="c" fileRef="true">
				<agpd:UI_Name>Configuration File</agpd:UI_Name>
				<agpd:UI_Description>File containing the main configuration parameters for SExtractor</agpd:UI_Description>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="PARAMETERS_NAME" type="binary" fileRef="true">
				<agpd:UI_Name>Parameter File</agpd:UI_Name>
				<agpd:UI_Description>A File containing the list of catalogue paramters that should be computed</agpd:UI_Description>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="CATALOG_NAME" type="VOTable" fileRef="true">
				<agpd:UI_Name>Derived output catalogue</agpd:UI_Name>
				<agpd:UI_Description>The catalogue of images derived from the input images</agpd:UI_Description>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="IMAGE_BAND" type="text" fileRef="false" commandSwitch="-IMAGEBAND">
				<agpd:UI_Name>Input image band</agpd:UI_Name>
				<agpd:UI_Description>The input image measuerment band. This is used to form  the column name in the output VOTable for the measured photometry. </agpd:UI_Description>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="FILTER_NAME" type="text" fileRef="true">
				<agpd:UI_Name>detection filter</agpd:UI_Name>
				<agpd:UI_Description>the convolution filter</agpd:UI_Description>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="DETECT_TYPE" type="text" fileRef="false">
				<agpd:UI_Name>detection type</agpd:UI_Name>
				<agpd:UI_Description/>
				<agpd:DefaultValue>CCD</agpd:DefaultValue>
				<agpd:OptionList>
					<agpd:OptionVal>CCD</agpd:OptionVal>
					<agpd:OptionVal>PHOTO</agpd:OptionVal>
				</agpd:OptionList>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="FLAG_IMAGE" type="FITS" fileRef="true">
				<agpd:UI_Name>flag image</agpd:UI_Name>
				<agpd:UI_Description/>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="DETECT_MINAREA" type="integer" fileRef="false">
				<agpd:UI_Name>minimum detection area</agpd:UI_Name>
				<agpd:UI_Description>minimum number of pixels above threshold</agpd:UI_Description>
				<agpd:DefaultValue>5</agpd:DefaultValue>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="DETECT_THRESH" type="double">
				<agpd:UI_Name>Detection Threshold</agpd:UI_Name>
				<agpd:UI_Description>sigmas or threshold,ZP in mag.arcsec-2</agpd:UI_Description>
				<agpd:DefaultValue>1.5</agpd:DefaultValue>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="ANALYSIS_THRESH" type="double" fileRef="false">
				<agpd:UI_Name>Analysis Threshold</agpd:UI_Name>
				<agpd:UI_Description>sigmas or threshold,ZP in mag.arcsec-2</agpd:UI_Description>
				<agpd:DefaultValue>1.5</agpd:DefaultValue>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="FILTER" type="boolean">
				<agpd:UI_Name>use filter</agpd:UI_Name>
				<agpd:UI_Description>apply filter for detection</agpd:UI_Description>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="DEBLEND_NTHRESH " type="integer" fileRef="false">
				<agpd:UI_Name>Deblend thresholds</agpd:UI_Name>
				<agpd:UI_Description> Number of deblending sub-thresholds</agpd:UI_Description>
				<agpd:DefaultValue>32</agpd:DefaultValue>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="DEBLEND_MINCONT" type="double" fileRef="false">
				<agpd:UI_Name>de-blend contrast</agpd:UI_Name>
				<agpd:UI_Description> Minimum contrast parameter for deblending</agpd:UI_Description>
				<agpd:DefaultValue>0.005</agpd:DefaultValue>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="CLEAN" type="boolean" fileRef="false">
				<agpd:UI_Name>Clean?</agpd:UI_Name>
				<agpd:UI_Description>Clean spurious detections? (Y or N)?
</agpd:UI_Description>
				<agpd:DefaultValue>Y</agpd:DefaultValue>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="CLEAN_PARAM" type="double" fileRef="false">
				<agpd:UI_Name>Cleaning efficiency</agpd:UI_Name>
				<agpd:UI_Description>Cleaning Efficiency</agpd:UI_Description>
				<agpd:DefaultValue>1.0</agpd:DefaultValue>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="MASK_TYPE" type="text" fileRef="false">
				<agpd:UI_Name>Mask type</agpd:UI_Name>
				<agpd:UI_Description> type of detection MASKing: can be one of                                "NONE", "BLANK" or "CORRECT"</agpd:UI_Description>
				<agpd:DefaultValue>CORRECT</agpd:DefaultValue>
				<agpd:OptionList>
					<agpd:OptionVal>NONE</agpd:OptionVal>
					<agpd:OptionVal>BLANK</agpd:OptionVal>
					<agpd:OptionVal>CORRECT</agpd:OptionVal>
				</agpd:OptionList>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="PHOT_APERTURES" type="integer" fileRef="false">
				<agpd:UI_Name>Photometry apertures</agpd:UI_Name>
				<agpd:UI_Description> MAG_APER aperture diameter(s) in pixels
</agpd:UI_Description>
				<agpd:DefaultValue>5</agpd:DefaultValue>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="PHOT_AUTOPARAMS" type="text" fileRef="false">
				<agpd:UI_Name>Photometry params</agpd:UI_Name>
				<agpd:UI_Description> MAG_AUTO parameters: &lt;Kron_fact&gt;,&gt;min_radius&gt;</agpd:UI_Description>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="MAG_ZEROPOINT" type="double" fileRef="false">
				<agpd:UI_Name> magnitude zero-point
</agpd:UI_Name>
				<agpd:UI_Description> magnitude zero-point
</agpd:UI_Description>
				<agpd:DefaultValue>0.0</agpd:DefaultValue>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="MAG_GAMMA" type="double" fileRef="false">
				<agpd:UI_Name>MAG_GAMMA</agpd:UI_Name>
				<agpd:UI_Description> gamma of emulsion (for photographic scans)</agpd:UI_Description>
				<agpd:DefaultValue>4.0</agpd:DefaultValue>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="GAIN" type="double" fileRef="false">
				<agpd:UI_Name>Gain</agpd:UI_Name>
				<agpd:UI_Description>detector gain in e-/ADU
</agpd:UI_Description>
				<agpd:DefaultValue>0.0</agpd:DefaultValue>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="PIXEL_SCALE" type="double" fileRef="false">
				<agpd:UI_Name>Pixel scale</agpd:UI_Name>
				<agpd:UI_Description>size of pixel in arcsec (0=use FITS WCS info)</agpd:UI_Description>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="SEEING_FWHM" type="double" commandPosition="-1" fileRef="false">
				<agpd:UI_Name>Seeing</agpd:UI_Name>
				<agpd:UI_Description>stellar FWHM in arcsec
</agpd:UI_Description>
				<agpd:DefaultValue>1.2</agpd:DefaultValue>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="STARNNW_NAME" type="binary" fileRef="true">
				<agpd:UI_Name>neural net file</agpd:UI_Name>
				<agpd:UI_Description> Neural-Network_Weight table filename
</agpd:UI_Description>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="BACK_SIZE" type="text" fileRef="false">
				<agpd:UI_Name>backgroud mesh size</agpd:UI_Name>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="BACK_FILTERSIZE " type="text" commandPosition="-1" fileRef="false">
				<agpd:UI_Name>background filter size</agpd:UI_Name>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="BACKPHOTO_TYPE" type="text" fileRef="false">
				<agpd:OptionList>
					<agpd:OptionVal>GLOBAL</agpd:OptionVal>
					<agpd:OptionVal>LOCAL</agpd:OptionVal>
				</agpd:OptionList>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="CHECKIMAGE_TYPE" type="text" fileRef="false">
				<agpd:UI_Name>Check image</agpd:UI_Name>
				<agpd:OptionList>
					<agpd:OptionVal>NONE</agpd:OptionVal>
					<agpd:OptionVal>BACKGROUND</agpd:OptionVal>
					<agpd:OptionVal>MINIBACKGROUND</agpd:OptionVal>
					<agpd:OptionVal>-BACKGROUND</agpd:OptionVal>
					<agpd:OptionVal>OBJECTS</agpd:OptionVal>
					<agpd:OptionVal>-OBJECTS</agpd:OptionVal>
					<agpd:OptionVal>SEGMENTATION</agpd:OptionVal>
					<agpd:OptionVal>APERTURES</agpd:OptionVal>
					<agpd:OptionVal>FILTERED</agpd:OptionVal>
				</agpd:OptionList>
			</CmdLineParameterDefn>
			<CmdLineParameterDefn name="CHECKIMAGE_NAME" type="FITS" commandPosition="-1" fileRef="true">
				<agpd:UI_Name>Checkimage name</agpd:UI_Name>
				<agpd:UI_Description>file containing the checkimage</agpd:UI_Description>
			</CmdLineParameterDefn>
		</Parameters>
		<Interfaces>
			<Interface name="simple">
				<input>
					<pref ref="DetectionImage"/>
					<pref ref="PhotoImage"/>
					<pref ref="config_file"/>
					<pref ref="PARAMETERS_NAME"/>
					<pref ref="FILTER_NAME"/>
					<pref ref="ANALYSIS_THRESH" minoccurs="0"/>
				</input>
				<output>
					<pref ref="CATALOG_NAME"/>
				</output>
			</Interface>
		</Interfaces>
		<ExecutionPath>/home/applications/tools/sextractor-2.3/sex</ExecutionPath>
		<Description>SExtractor is a program that builds a catalogue of objects from an astronomical image. Although it is particularly oriented towards reduction of large scale galaxy-survey data, it performs rather well on moderately crowded star fields. 

</Description>
		<ReferenceURL>http://terapix.iap.fr/cplt/oldSite/soft/sextractor/</ReferenceURL>
	</Application>
	<Application name="REGAUTHORITY.HyperZ" instanceClass="org.astrogrid.applications.commandline.hyperz.HyperZ">
