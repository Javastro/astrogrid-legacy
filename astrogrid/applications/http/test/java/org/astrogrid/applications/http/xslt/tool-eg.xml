<?xml version="1.0" encoding="UTF-8"?>
<!--example tool document-->
<tool name="Adder" interface="TheOneAndOnlyInterface" 
	xmlns="http://www.astrogrid.org/schema/AGWorkflow/v1">
	<input>
		<parameter name="x">
			<ns1:value 
				xmlns:ns1="http://www.astrogrid.org/schema/AGParameterDefinition/v1">
				3</ns1:value>
		</parameter>
		<parameter name="y">
			<ns2:value 
				xmlns:ns2="http://www.astrogrid.org/schema/AGParameterDefinition/v1">
				5</ns2:value>
		</parameter>
	</input>
	<output>
		<parameter name="sum">
			<ns3:value 
				xmlns:ns3="http://www.astrogrid.org/schema/AGParameterDefinition/v1"></ns3:value>
		</parameter>
	</output>
</tool>