<?xml version="1.0" encoding="utf-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<extend>../../maven-base/project.xml</extend>
<id>astrogrid-cea-client</id>
<name>CEA Client</name>
<description>Client / Delegate library for communicatng with CEA services</description>
<shortDescription>AstroGrid CEA Client</shortDescription>
<build>
