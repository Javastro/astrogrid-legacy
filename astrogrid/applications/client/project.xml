    <id>axis</id>
    <version>1.1</version>
  </dependency>
  <dependency>
    <id>wsdl4j</id>
    <version>1.0</version>
  </dependency>
  <dependency>
    <id>saaj</id>
    <version>1.0</version>
    <jar>saaj.jar</jar>
  </dependency>
  <dependency>
    <id>commons-logging</id>
    <version>1.0.3</version>
  </dependency>
  <dependency>
    <id>commons-discovery</id>
