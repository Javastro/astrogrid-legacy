<?xml version="1.0" encoding="iso-8859-1" ?>
<!--
<?xml-stylesheet type="text/xsl" href="xform.xsl"?>
-->
<html xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xf="http://www.w3.org/2002/xforms" xmlns:xforms="http://www.w3.org/2002/xforms" xmlns="http://www.w3.org/1999/xhtml" xmlns:xhtml="http://www.w3.org/1999/xhtml">
<head>
<!--
<form xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xforms="http://www.w3.org/2002/xforms">
-->
<xforms:model id="model1" xmlns:xforms="http://www.w3.org/2002/xforms">
<xforms:instance>
</xforms:instance>
<xforms:submission method="get" action="http://xforms.zeninteractif.com/get.php" id="s00"/>
<xforms:bind ref="vr:title" required="true()"  />
<xforms:bind ref="vr:curation/vr:publisher" required="true()"  />
<xforms:bind ref="identifier" required="true()" type="vr:IdentifierURI" />
<xforms:bind ref="vr:curation/vr:contact/vr:email" type="xsd:email"  />
</xforms:model>

</head>
<body>
<div id="main">

<xforms:select ref="@status" appearance="minimal">
   <xforms:label>Status</xforms:label>
   <xforms:choices>
   <xforms:label>Status</xforms:label>
   <xforms:item>
      <xforms:label>Active</xforms:label><xforms:value>active</xforms:value>
   </xforms:item>
   <xforms:item>
      <xforms:label>Inactive</xforms:label><xforms:value>inactive</xforms:value>
   </xforms:item>
   <xforms:item>
      <xforms:label>Deleted</xforms:label><xforms:value>deleted</xforms:value>
   </xforms:item>
   </xforms:choices>
</xforms:select>

<xforms:input ref="vr:title">
   <xforms:label>Title:</xforms:label>
</xforms:input>

<xforms:input ref="vr:shortname">
   <xforms:label>Short Name:</xforms:label>
</xforms:input>

<xforms:input ref="vr:identifier">
   <xforms:label>Identifier</xforms:label>
</xforms:input>
<!--
<xforms:group>
   <label>
       Curation
   </label>
   -->
<xforms:group nodeset="vr:curation">
<xforms:label>Curation</xforms:label>
   
   <xforms:input ref="vr:publisher">
      <xforms:label>Publisher:</xforms:label>
   </xforms:input>
   
   <xforms:input ref="vr:publisher/@ivo-id">
      <xforms:label>Publisher Identifier:</xforms:label>
   </xforms:input>
   
   <xforms:input ref="vr:creator/vr:name">
      <xforms:label>Creator Name:</xforms:label>
   </xforms:input>
   
   <xforms:input ref="vr:creator/vr:logo">
      <xforms:label>Creator Logo:</xforms:label>
   </xforms:input>


   <xforms:repeat id="contributerRepeat" nodeset="vr:curation/vr:contributer" >
      <xforms:input ref=".">
         <xforms:label>Contributor:</xforms:label>
      </xforms:input>
      <xforms:input ref="./@ivo-id">
         <xforms:label>Contributor Identifier:</xforms:label>
      </xforms:input>      
   </xforms:repeat>
   
   <xforms:trigger id="addContributer">
      <xforms:label>Add Contributer</xforms:label>
      <xforms:insert nodeset="vr:curation/vr:contributer"
         at="last()" position="after" ev:event="DOMActivate" />
    </xforms:trigger>

   <xforms:repeat id="curationDateRepeat" nodeset="vr:curation/vr:date" >
      <xforms:input ref=".">
         <xforms:label>Date:</xforms:label>
      </xforms:input>

   </xforms:repeat>
   <xforms:trigger id="addDate">
      <xforms:label>Add Date</xforms:label>
      <xforms:insert nodeset="vr:curation/vr:date"
         at="last()" position="after" ev:event="DOMActivate" />
    </xforms:trigger>

   <xforms:input ref="vr:curation/vr:version">
      <xforms:label>Version:</xforms:label>
   </xforms:input>
   
</xforms:group>
  <xforms:group nodeset="vr:curation/vr:contact">
  <xforms:label>Contact</xforms:label>

   <xforms:input ref="vr:name">
   <xforms:label>Name:</xforms:label>
   </xforms:input>
   
   <xforms:input ref="vr:name/@ivo-id">
   <xforms:label>Name Identifier:</xforms:label>
   </xforms:input>
   
   <xforms:input ref="vr:adress">
   <xforms:label>Adress:</xforms:label>
   </xforms:input>
   
   <xforms:input ref="vr:email">
   <xforms:label>Email:</xforms:label>
   </xforms:input>
   
   <xforms:input ref="vr:telephone">
   <xforms:label>Adress:</xforms:label>
   </xforms:input>
</xforms:group>

<!--
<xforms:group>
   <label>Content</label>
   -->
  <xforms:group nodeset="vr:content">
  <xforms:label>Content</xforms:label>
   

   <xforms:repeat id="contentSubject" nodeset="vr:subject" >
      <xforms:input ref=".">
         <xforms:label>Subject:</xforms:label>
      </xforms:input>
   </xforms:repeat>
   <xforms:trigger id="addSubject">
      <xforms:label>Add Subject</xforms:label>
      <xforms:insert nodeset="vr:subject"
         at="last()" position="after" ev:event="DOMActivate" />
    </xforms:trigger>


   <xforms:input ref="vr:description">
   <xforms:label>Description:</xforms:label>
   </xforms:input>
   
   <xforms:input ref="vr:source">
   <xforms:label>Source:</xforms:label>
   </xforms:input>
   
   <xforms:input ref="vr:source/@format">
   <xforms:label>Source Format:</xforms:label>
   </xforms:input>
   
   <xforms:input ref="vr:referenceURL">
   <xforms:label>Reference URL:</xforms:label>
   </xforms:input>
   
   <xforms:select ref="vr:type" appearance="minimal">
   <xforms:label>Type</xforms:label>
   <xforms:choices>
   <xforms:label>Type</xforms:label>
   <xforms:item>
      <xforms:label>Archive</xforms:label><xforms:value>Archive</xforms:value>
   </xforms:item>
   <xforms:item>
      <xforms:label>Bibliography</xforms:label><xforms:value>Bibliography</xforms:value>
   </xforms:item>
   <xforms:item>
      <xforms:label>Catalog</xforms:label><xforms:value>Catalog</xforms:value>
   </xforms:item>
   </xforms:choices>
   </xforms:select>

<!--
   <xforms:repeat id="contentLevel" nodeset="vr:content/vr:contentLevel" >
      <xforms:select ref="." appearance="minimal">
      <xforms:label>Content Level</xforms:label>
      
      <xforms:choices>
      <xforms:label>Content Level</xforms:label>
      <xforms:item>
         <xforms:label>General</xforms:label><xforms:value>General</xforms:value>
      </xforms:item>
      <xforms:item>
         <xforms:label>Elementary Education</xforms:label><xforms:value>Elementary Education</xforms:value>
      </xforms:item>
      <xforms:item>
         <xforms:label>Middle School Education</xforms:label><xforms:value>Middle School Education</xforms:value>
      </xforms:item>
      </xforms:choices>
      </xforms:select>
   </xforms:repeat>
   <xforms:trigger id="addContentLevel">
      <xforms:label>Add Content Level</xforms:label>
      <xforms:insert nodeset="vr:content/vr:contentLevel"
         at="last()" position="after" ev:event="DOMActivate" />
    </xforms:trigger>
-->

   <xforms:repeat id="relationShip" nodeset="vr:content/vr:relationship" >
   
      <!--
      <xforms:select ref="./vr:relationshipType" appearance="minimal">
         <xforms:label>Relationship Type</xforms:label>
         <xforms:choices>
            <xforms:label>Type</xforms:label>
            <xforms:item>
               <xforms:label>Derived From</xforms:label><xforms:value>derived-from</xforms:value>
            </xforms:item>
            <xforms:item>
               <xforms:label>Service For</xforms:label><xforms:value>service-for</xforms:value>
            </xforms:item>
         </xforms:choices>
      </xforms:select>
      -->   
      <xforms:input ref="./vr:relatedResource/@ivo-id">
      <xforms:label>Related Resource Identifier:</xforms:label>
      </xforms:input>
      
      <xforms:input ref="./vr:relatedResource">
      <xforms:label>Related Resource Name:</xforms:label>
      </xforms:input>
   </xforms:repeat>
   <xforms:trigger id="addRelationShip">
      <xforms:label>Add RelationShip</xforms:label>
      <xforms:insert nodeset="vr:content/vr:relationship"
         at="last()" position="after" ev:event="DOMActivate" />
    </xforms:trigger>

</xforms:group>

<xforms:trigger>
   <xforms:label>Cancel</xforms:label>
   <xforms:reset ev:event="DOMActivate" model="model0"/>
</xforms:trigger>  

<xforms:submit submission="s00">     
   <xforms:label>Register</xforms:label>
</xforms:submit>  

<!--
</form>
-->
</div>
</body>
</html>