<properties>
   <!--+
       | Configuration properties for the community service.
       | These values are for illustration only.
       | The actual values are filled in by the Ant build script.
       |
       +-->
   <category name="org.astrogrid.registry">

      <!--
      	Used on the client side.
      -->      
      <property name="dummy.mode.on" value="true"/>
      
      <!--
      	The update web service url in which all data updated/added/ is inserted to a web service method.
      	Used on the client side.
      -->      
      <property name="publish.registry.update.url" value="http://localhost:8080/axis/services/RegistryAdmin"/>
      
      <!--
      	The harvest web service url in which all harvesting occurrs.
      -->            
      <property name="registry.harvest.url" value="http://localhost:8080/axis/services/RegistryHarvest"/>                  
     
      <!--
      	Web service url to query a registry. Used for the client side.
      -->
      <property name="search.registry.query.url" value="http://localhost:8080/axis/services/Registry"/>
      
      <!--
      	Not used anymore.  Allowed the ability to update to a search registry.  But now it is stated a portal
      	is associated to 1 and 1 registry only.  So it uses the publish.registry.update.url property up above.
      -->
      <property name="search.registry.update.url" value="http://localhost:8080/axis/services/RegistryAdmin"/>      
            
      <!--
      	Organisation template file used for the client for adding or getting text nodes and attribute nodes for the query
