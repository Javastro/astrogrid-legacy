<?xml version = "1.0" encoding = "UTF8"?>
<job name = "jl011">
   <userid>jlusted</userid>
   <community>leicester</community>
   <jobstep name="BETWEEN" >
      <query>  
<!--============================================== 
    SELECT URA, UDEC, NDETS 
    FROM USNOB 
    WHERE URA BETWEEN 234.56 AND 234.6 
          AND 
          UDEC BETWEEN -12.34 AND -12.45 
          AND 
          NDETS > 4 
===============================================--> 
         <from>         
            <catalog name="USNOB">         
               <service name="AGHomeService" url="http://hydra.star.le.ac.uk:8080/axis/services/DatasetAgent" />
               <table>USNOB</table>
            </catalog>
         </from>
         <return>
            <field name="URA" type="COLUMN"/>
            <field name="UDEC" type="COLUMN"/>
            <field name="NDETS" type="COLUMN"/>
         </return>
         <criteria>
            <operation name="AND">
                <operation name="BETWEEN">
                   <field name="URA" type="COLUMN"/>
                   <field>234.56</field>
                   <field>234.6</field>
                </operation>
                <operation name="BETWEEN">
                   <field name="UDEC" type="COLUMN"/>
                   <field>-12.34</field>
                   <field>-12.45</field>
                </operation>
                <operation name="GREATER_THAN">
                   <field name="NDETS" type="COLUMN"/>
                   <field>4</field>
                </operation>
            </operation>
         </criteria>
      </query>
   </jobstep>
</job>
