<?xml version = "1.0" encoding = "UTF8"?>
<job name = "Cone Query with AND">
   <userid>jlusted</userid>
   <community>leicester</community>
   <jobstep name="cone step" >
      <query>      
         <from>         
            <catalog name="USNOB">         
               <service name="AGHomeService" url="http://hydra.star.le.ac.uk:8080/axis/services/DatasetAgent" />
               <table>USNOB</table>
            </catalog>
         </from>
         <return>
            <field name="URA" type="COLUMN"/>
            <field name="UDEC" type="COLUMN"/>
            <field name="PMPROB" type="COLUMN"/>
         </return>
         <criteria>
            <operation name="AND">
               <operation name="CONE">
                  <field name="RA">234.56</field>
                  <field name="DEC">-12.34</field>
                  <field name="RADIUS">0.01</field>
               </operation>
               <operation name="GREATER_THAN">
                  <field name="PMPROB" type="COLUMN"/>
                  <field>5</field> 
               </operation>
            </operation>
         </criteria>
      </query>
   </jobstep>
</job>
