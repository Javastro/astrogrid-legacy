<?xml version = "1.0" encoding = "UTF8"?>
<job name = "jl007">
   <userid>jlusted</userid>
   <community>leicester</community>
   <jobstep name="aconesearch" >
      <query>      
         <from>         
            <catalog name="USNOB">         
               <service name="AGHomeService" url="http://hydra.star.le.ac.uk:8080/axis/services/DatasetAgent" />
               <table>USNOB</table>
            </catalog>
         </from>
         <return>
            <field name="COLUMN_ONE" type="COLUMN"/>
            <field name="COLUMN_TWO" type="COLUMN"/>
         </return>
         <criteria>
            <operation name="AND">
               <operation name="GREATER_THAN_OR_EQUALS">
                  <field name="COLUMN_6" type="COLUMN"/>
                  <field>60</field>
               </operation>
               <operation name="LESS_THAN_OR_EQUALS">
                  <field name="COLUMN_7" type="COLUMN"/>
                  <field>70</field>
               </operation>
               <operation name="OR">
                  <operation name="EQUALS">
                     <field name="COLUMN_11" type="COLUMN"/>
                     <field>110</field>
                  </operation>
                  <operation name="NOT_EQUALS">
                     <field name="COLUMN_12" type="COLUMN"/>
                     <field name="COLUMN_13" type="COLUMN"/>
                  </operation>
               </operation>
               <operation name="GREATER_THAN">
                  <field name="COLUMN_8" type="COLUMN"/>
                  <field>80</field>
               </operation>
               <operation name="LESS_THAN">
                  <field name="COLUMN_9" type="COLUMN"/>
                  <field>90</field>
               </operation>
               <operation name="NOT_EQUALS">
                  <field name="COLUMN_10" type="COLUMN"/>
                  <field>100</field>
               </operation>
            </operation>
         </criteria>
      </query>
   </jobstep>
</job>
