               <service name="AGHomeService" url="http://hydra.star.le.ac.uk:8080/axis/services/DatasetAgent" />
               <table>USNOB</table>
            </catalog>
         </from>
         <return>
            <field name="MIN(URA)" type="COLUMN"/>
            <field name="MAX(URA)" type="COLUMN"/>
