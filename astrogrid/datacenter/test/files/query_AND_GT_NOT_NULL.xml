<?xml version = "1.0" encoding = "UTF8"?>
<job name = "jl010">
   <userid>jlusted</userid>
   <community>leicester</community>
   <jobstep name="not null" >
      <query>  
<!-- SELECT * FROM USNOB WHERE TYCHO > 0 AND NMAG NOT NULL --> 
         <from>         
            <catalog name="USNOB">         
               <service name="AGHomeService" url="http://hydra.star.le.ac.uk:8080/axis/services/DatasetAgent" />
               <table>USNOB</table>
            </catalog>
         </from>
         <return>
            <field name="*" type="COLUMN"/>
         </return>
         <criteria>
            <operation name="AND">
                <operation name="GREATER_THAN">
                   <field name="TYCHO" type="COLUMN"/>
                   <field>0</field>
                </operation>
                <operation name="NOT_NULL">
                  <field name="NMAG" type="COLUMN"/>
                </operation>
            </operation>
         </criteria>
      </query>
   </jobstep>
</job>
