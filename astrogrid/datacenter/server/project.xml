<?xml version="1.0" encoding="ISO-8859-1"?>
<project>

<extend>${basedir}/../project-base.xml</extend>
  <!-- the version of maven's project object model -->
  <pomVersion>3</pomVersion>

  <!-- a unique name for this project -->
  <id>astrogrid-datacenter-server</id>
  <name>AstroGrid DataCenter Server</name>
  <currentVersion>HEAD</currentVersion>

	<description><![CDATA[
	 This project contains the datacenter server framework
	 For specific server implementations, see the axis-datacenter-implementation project
	 	]]>
	</description>

  <!-- must be on a single line -->
  <shortDescription>Datacenter server framework</shortDescription>

  <siteDirectory>/var/www/www/maven/build/datacenter/server</siteDirectory>

  <repository>
    <connection>
      scm:cvs:pserver:anoncvs@cvs.astrogrid.org:/devel:astrogrid/datacenter/server
    </connection>
    <developerConnection>
      scm:cvs:ext:${maven.username}@cvs.astrogrid.org:/devel:astrogrid/datacenter/server
    </developerConnection>
  </repository>



  <!-- jar files the project is dependent on -->
  <dependencies>

	<dependency>
		<groupId>astrogrid-datacenter</groupId>
		<artifactId>astrogrid-datacenter-client</artifactId>
		<version>SNAPSHOT</version>
		    <properties>
      <war.bundle>true</war.bundle>
    </properties>
	</dependency>
	    <dependency>
        <id>astrogrid-mySpace</id>
        <version>SNAPSHOT</version>
	    <properties>
      <war.bundle>true</war.bundle>
    </properties>
    </dependency>

    <dependency>
      <id>breezetk</id>
      <version>UNKNOWN</version>
      <jar>breezetk.jar</jar>
          <properties>
      <war.bundle>true</war.bundle>
    </properties>
    </dependency>

    <dependency>
      <id>objectwiz-votable</id>
      <version>UNKNOWN</version>
      <jar>objectwiz-votable.jar</jar>
          <properties>
      <war.bundle>true</war.bundle>
    </properties>
    </dependency>

    <dependency>
      <id>VOTableUtil</id>
      <version>UNKNOWN</version>
      <jar>VOTableUtil.jar</jar>
          <properties>
      <war.bundle>true</war.bundle>
    </properties>
    </dependency>

    <dependency><!-- hsqldb - for testing -->
        <id>hsqldb</id>
        <version>1.7.1</version>
	    <properties>
      <war.bundle>true</war.bundle>
    </properties>
    </dependency>
  </dependencies>

  <!-- build information for the project -->

</project>

