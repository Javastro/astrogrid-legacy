<!-- Select * from CASU_INT_DSA as casu WHERE (casu.RA > 56.45 AND casu.RA < 57.05) AND (casu.DEC > 23.567 AND casu.DEC < 24.167) -->
<!-- Should work on Dummy SampleStars database -->
<Select  xmlns="http://adql.ivoa.net/v0.73">
   <SelectionList>
      <All/>
   </SelectionList>
   <From>
      <Table Name="SampleStars" Alias="s"/>
   </From>
   <Where>
      <And>
         <And>
            <Compare Comparison="&gt;">
               <Column Table="s" Name="RA"/>
               <Atom><Real>56.45</Real></Atom>
            </Compare>
            <Compare Comparison="&lt;">
               <Column Table="s" Name="RA"/>
               <Atom><Real>57.05</Real></Atom>
            </Compare>
         </And>
         <And>
            <Compare Comparison="&gt;">
               <Column Table="s" Name="DEC"/>
               <Atom><Real>23.567</Real></Atom>
            </Compare>
            <Compare Comparison="&lt;">
               <Column Table="s" Name="DEC"/>
               <Atom><Real>24.167</Real></Atom>
            </Compare>
         </And>
      </And>
   </Where>
</Select>

