<!-- Select t.a, g.d from Tab a, Tab d where a.d < d.e and a.f < d.f order by t.f, g.b -->
<Select>
   <SelectionList>
      <Column Table="t" Name="a"/>
      <Column Table="g" Name="d"/>
   </SelectionList>
   <From>
      <Table Name="Tab" Alias="a"/>
      <Table Name="Tab" Alias="d"/>
   </From>
   <Where>
      <And>
         <Compare Comparison="<">
            <Column Table="a" Name="d"/>
            <Column Table="d" Name="e"/>
         </Compare>
         <Compare Comparison="<">
            <Column Table="a" Name="f"/>
            <Column Table="d" Name="f"/>
         </Compare>
      </And>
   </Where>
   <OrderBy>
      <Item>
         <Column Table="t" Name="f"/>
      </Item>
      <Item>
         <Column Table="g" Name="b"/>
      </Item>
   </OrderBy>
</Select>
