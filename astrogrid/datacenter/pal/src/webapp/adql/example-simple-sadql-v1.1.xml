<ADQ xmlns="http://astrogrid.org/sadql/v1.1">
   <From>
      <Column>
         <Alias>RA</Alias>
         <Table>SampleStars</Table>
         <Column>RA</Column>
      </Column>
      <Column>
         <Alias>DEC</Alias>
         <Table>SampleStars</Table>
         <Column>DEC</Column>
      </Column>
      <Column>
         <Alias>MAG</Alias>
         <Table>SampleStars</Table>
         <Column>MAG</Column>
      </Column>
   </From>
   <Return>
      <Set>
         <All/>
      </Set>
   </Return>
   <Where>
         <Compare>
            <Alias>MAG</Alias>
            <GREATERTHAN/>
            <RealNumber>19.5</RealNumber>
         </Compare>
         <AND/>
         <Compare>
            <Math>
               <Alias>RA</Alias>
               <SUBTRACT/>
               <Alias>DEC</Alias>
            </Math>
            <LESSTHAN/>
            <RealNumber>2.2</RealNumber>
         </Compare>
   </Where>
</ADQ>
