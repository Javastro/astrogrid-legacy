<Select xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.ivoa.net/xml/ADQL/v0.7.4">
  <SelectionList>
    <Item xsi:type="allSelectionItemType" />
  </SelectionList>
  <From>
    <Table xsi:type="tableType" Name="Tab" Alias="t" />
  </From>
</Select>



<Select xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xmlns="http://tempuri.org/adql">

  <SelectionAll/>
 
  <TableClause>
 
   <FromClause>
      <TableReference>
        <Table>
          <Name>CASU_INT_DSA</Name>
          <AliasName>t</AliasName>
        </Table>
      </TableReference>
    </FromClause>

    <WhereClause>
      <IntersectionSearch>
        <FirstCondition xsi:type="PredicateSearch">
          <ComparisonPred>

            <FirstExpr xsi:type="ColumnExpr">
              <SingleColumnReference>
                <TableName>t</TableName>
                <Name>fluxErr_z</Name>
              </SingleColumnReference>
            </FirstExpr>

            <Compare>&lt;</Compare>

            <SecondExpr xsi:type="AtomExpr">
               <Value>
                   <NumberLiteral>
                       <ApproxNum>
                           <Value>0.5</Value>
                       </ApproxNum>
                   </NumberLiteral>
               </Value>
            </SecondExpr>

          </ComparisonPred>
        </FirstCondition>
        
        <SecondCondition xsi:type="Circle">
          <Ra>
            <Value>56.75</Value>
          </Ra>
          <Dec>
            <Value>23.867</Value>
          </Dec>
          <Radius>
            <Value>0.3</Value>
          </Radius>
        </SecondCondition>

      </IntersectionSearch>
    </WhereClause>
  </TableClause>

</Select>

