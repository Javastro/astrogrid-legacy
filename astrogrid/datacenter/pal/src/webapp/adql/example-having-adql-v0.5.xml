<!-- SQL equivalent
SELECT s.ID, s.MAG FROM SampleStars s, SampleGalaxies g HAVING  s.RA <> g.DEC
-->
<Select xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xmlns="http://tempuri.org/adql"
>
  <SelectionList>
    <ColumnExpr>
      <SingleColumnReference>
        <TableName>s</TableName>
        <Name>ID</Name>
      </SingleColumnReference>
    </ColumnExpr>
    <ColumnExpr>
      <SingleColumnReference>
        <TableName>s</TableName>
        <Name>MAG</Name>
      </SingleColumnReference>
    </ColumnExpr>
  </SelectionList>
  <TableClause>
    <FromClause>
      <TableReference>
        <Table>
          <Name>SampleStars</Name>
          <AliasName>s</AliasName>
        </Table>
        <Table>
          <Name>SampleGalaxies</Name>
          <AliasName>g</AliasName>
        </Table>
      </TableReference>
    </FromClause>
    <HavingClause>
      <PredicateSearch>
        <ComparisonPred>
          <FirstExpr xsi:type="ColumnExpr">
            <SingleColumnReference>
              <TableName>s</TableName>
              <Name>RA</Name>
            </SingleColumnReference>
          </FirstExpr>
          <Compare>&lt;&gt;</Compare>
          <SecondExpr xsi:type="ColumnExpr">
            <SingleColumnReference>
              <TableName>g</TableName>
              <Name>RA</Name>
            </SingleColumnReference>
          </SecondExpr>
        </ComparisonPred>
      </PredicateSearch>
    </HavingClause>
  </TableClause>
</Select>
