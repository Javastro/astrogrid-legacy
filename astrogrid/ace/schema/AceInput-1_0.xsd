<?xml version="1.0" encoding="UTF-8"?>
<schema
   targetNamespace="http://www.astrogrid.org/namespace/AceInput-1_0"
   xmlns="http://www.w3.org/2001/XMLSchema"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns:ace="http://www.astrogrid.org/namespace/AceInput-1_0"
   elementFormDefault="unqualified">

   <annotation>
      <documentation xml:lang="en">
            Toplevel schema for Ace SExtractor Service input, intended for
            AstroGrid Ace demonstrator.  Configured for SExtractor v2.2.2.
      </documentation>
   </annotation>

   <!-- THE ROOT ELEMENT OF THE XML DOCUMENT -->
   <element name="AceInputDoc" type="ace:AceInputDocType"/>

   <!-- TYPE DEFINITIONS FOR THIS SCHEMA FOLLOW -->

   <!-- OVERALL DOCUMENT STRUCTURE -->
   <!-- NB: TOPLEVEL ELEMENTS UNDER AceInputDocType CAN APPEAR IN ANY ORDER -->
   <complexType name="AceInputDocType">
      <all>

<!-- ======================================================================
     ======================================================================
      NB: THIS SECTION REPRESENTS SPECIALISED PARAMETERS FOR ACE ITSELF
     ======================================================================
     ======================================================================
-->
         <element name="Wavelength" minOccurs="1" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" type="string"
                        minOccurs="1" maxOccurs="1"/>
               </sequence>
            </complexType>
         </element>

         <element name="Passband" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" type="string"
                        minOccurs="1" maxOccurs="6"/>
               </sequence>
            </complexType>
         </element>

<!-- ======================================================================
     ======================================================================
      NB: THIS SECTION REPRESENTS COMMAND-LINE PARAMETERS FOR SEXTRACTOR
     ======================================================================
     ======================================================================
-->
         <!-- REQUIRED INPUT IMAGE FILE NAMES -->
         <!-- NB: IF IDENTICAL, SExtractor WILL RUN IN "SINGLE-IMAGE" MODE -->

         <element name="ImageToCatalog" minOccurs="1" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" type="string"
                        minOccurs="1" maxOccurs="1"/>
               </sequence>
            </complexType>
         </element>

         <element name="ImageToMeasure" minOccurs="1" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" type="string"
                        minOccurs="1" maxOccurs="1"/>
               </sequence>
            </complexType>
         </element>


<!-- ======================================================================
     ======================================================================
      NB: THIS SECTION REPRESENTS PARAMETERS FROM THE SEXTRACTOR
        CONFIGURATION PARAMETERS FILE (*.sex)
        These are given in the SExtractor source file "prefs.h".

      THE FOLLOWING SECTION REPRESENTS PARAMETERS FROM THE
        SEXTRACTOR CATALOG PARAMETERS FILE (*.param).
        These are given in the SExtractor source file "param.h".
     ======================================================================
     ======================================================================
-->

      <!-- ================================================================
         { "ANALYSIS_THRESH", P_FLOATLIST, prefs.thresh, 0,0, -BIG, BIG,
            {""}, 1, 2, &prefs.nthresh}
      -->
         <element name="ANALYSIS_THRESH" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="2">
                     <simpleType>
                        <restriction base="double">
                           <minInclusive value="-1.0E30"/>
                           <maxInclusive value="1.0E30"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"ASSOC_DATA", P_INTLIST, prefs.assoc_data, 0, 1000000,0.0,0.0,
             {""}, 1,MAXLIST, &prefs.nassoc_data} /*MAXLIST=32*/
      -->
         <element name="ASSOC_DATA" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="32">
                     <simpleType>
                        <restriction base="nonNegativeInteger">
                           <maxInclusive value="1000000"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"ASSOC_NAME", P_STRING, prefs.assoc_name}
      -->
         <element name="ASSOC_NAME" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" type="string"
                     minOccurs="1" maxOccurs="1"/>
               </sequence>
            </complexType>
         </element>
         

      <!-- ================================================================
          {"ASSOC_PARAMS", P_INTLIST, prefs.assoc_param, 1, 1000000,0.0,0.0,
              {""}, 2,3, &prefs.nassoc_param}
      -->
         <element name="ASSOC_PARAMS" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="2" maxOccurs="3">
                     <simpleType>
                        <restriction base="nonNegativeInteger">
                           <maxInclusive value="1000000"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"ASSOC_RADIUS", P_FLOAT, &prefs.assoc_radius, 0,0, 1e-10,1e+10},
      -->
         <element name="ASSOC_RADIUS" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="1">
                     <simpleType>
                        <restriction base="double">
                           <minInclusive value="1.0E-10"/>
                           <maxInclusive value="1.0E10"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"ASSOC_TYPE", P_KEY, &prefs.assoc_type, 0,0, 0.0,0.0,
         {"FIRST", "NEAREST", "MEAN", "MAG_MEAN", "SUM", "MAG_SUM",
            "MIN", "MAX", ""}}
      -->
         <element name="ASSOC_TYPE" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="1">
                     <simpleType>
                        <restriction base="string">
                           <enumeration value="FIRST"/>
                           <enumeration value="NEAREST"/>
                           <enumeration value="MEAN"/>
                           <enumeration value="MAG_MEAN"/>
                           <enumeration value="SUM"/>
                           <enumeration value="MAG_SUM"/>
                           <enumeration value="MIN"/>
                           <enumeration value="MAX"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"ASSOCSELEC_TYPE", P_KEY, &prefs.assocselec_type, 0,0, 0.0,0.0,
         {"ALL","MATCHED","-MATCHED",""}}
      -->
         <element name="ASSOCSELEC_TYPE" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="1">
                     <simpleType>
                        <restriction base="string">
                           <enumeration value="ALL"/>
                           <enumeration value="MATCHED"/>
                           <enumeration value="-MATCHED"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"BACK_FILTERSIZE", P_INTLIST, prefs.backfsize, 1,11, 0.0,0.0,
          {""}, 1,2, &prefs.nbackfsize}
      -->
         <element name="BACK_FILTERSIZE" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="2">
                     <simpleType>
                        <restriction base="positiveInteger">
                           <maxInclusive value="11"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"BACK_FILTTHRESH", P_FLOAT, &prefs.backfthresh, 0,0, 0.0,BIG}

         NB: THIS ONE ISN'T IN THE PRINTED DOCUMENTATION !
      -->
         <element name="BACK_FILTTHRESH" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="1">
                     <simpleType>
                        <restriction base="double">
                           <minInclusive value="0.0"/>
                           <maxInclusive value="1.0E30"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"BACK_SIZE", P_INTLIST, prefs.backsize, 1,2000000000, 0.0,0.0,
            {""}, 1,2, &prefs.nbacksize}
      -->
         <element name="BACK_SIZE" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="2">
                     <simpleType>
                        <restriction base="positiveInteger">
                           <maxInclusive value="2000000000"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"BACK_TYPE", P_KEYLIST, prefs.back_type, 0,0, 0.0,0.0,
            {"AUTO","MANUAL",""}
               1, 2, &prefs.nback_type}

         NB: DIFFERS FROM DOC! "AUTO" is "INTERNAL" in doco!
      -->
         <element name="BACK_TYPE" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="2">
                     <simpleType>
                        <restriction base="string">
                           <enumeration value="AUTO"/>
                           <enumeration value="MANUAL"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"BACK_VALUE", P_FLOATLIST, prefs.back_val, 0,0, -BIG,BIG,
            {""}, 1, 2, &prefs.nback_val}
      -->
         <element name="BACK_VALUE" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="2">
                     <simpleType>
                        <restriction base="double">
                           <minInclusive value="-1.0E30"/>
                           <maxInclusive value="1.0E30"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
        {"BACKPHOTO_THICK", P_INT, &prefs.pback_size, 1, 256}
      -->
         <element name="BACKPHOTO_THICK" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="1">
                     <simpleType>
                        <restriction base="positiveInteger">
                           <maxInclusive value="256"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
        {"BACKPHOTO_TYPE", P_KEY, &prefs.pback_type, 0,0, 0.0,0.0,
           {"GLOBAL","LOCAL",""}}
      -->
         <element name="BACKPHOTO_TYPE" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="1">
                     <simpleType>
                        <restriction base="string">
                           <enumeration value="GLOBAL"/>
                           <enumeration value="LOCAL"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"CATALOG_NAME", P_STRING, prefs.cat_name}

         This element (output file name) is not needed for Ace
         (which returns VOTable).
      -->

      <!-- ================================================================
         {"CATALOG_TYPE", P_KEY, &prefs.cat_type, 0,0, 0.0,0.0,
            {"NONE", "ASCII","ASCII_HEAD", "ASCII_SKYCAT", "FITS_LDAC",
               "FITS_BINIMHEAD","FITS_NOIMHEAD","FITS_1.0",""}}
         NB: THE LAST 3 KEYWORDS AREN'T IN THE PRINTED DOCO!

         This element (output file type) is not needed for Ace
         (which returns VOTable).
      -->

      <!-- ================================================================
         {"CHECKIMAGE_NAME", P_STRINGLIST, prefs.check_name, 0,0,0.0,0.0,
            {""}, 0, MAXCHECK, &prefs.ncheck_name} /* MAXCHECK = 32 */

         NB: CONFLICT WITH DOCO - DOCO CLAIMS <=16 NAMES ONLY
         NB: USING DOCO CONVENTION (<=16)
         NB: ENSURING AT LEAST ONE ARG. IS PRESENT (IF NOT, OMIT THIS ELT)
      -->
         <element name="CHECKIMAGE_NAME" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" type="string"
                     minOccurs="1" maxOccurs="16"/>
               </sequence>
            </complexType>
         </element>

      <!-- ================================================================
         {"CHECKIMAGE_TYPE", P_KEYLIST, prefs.check_type, 0,0, 0.0,0.0,
            {"NONE", "IDENTICAL",
               "BACKGROUND", "BACKGROUND_RMS", "MINIBACKGROUND",
               "MINIBACK_RMS", "-BACKGROUND",
               "FILTERED", "OBJECTS", "APERTURES", "SEGMENTATION",
               "ASSOC",
               "-OBJECTS", "-PSF_PROTOS", "PSF_PROTOS",
               "-PC_CONVPROTOS", "PC_CONVPROTOS", "PC_PROTOS",
               ""}, 0, 17, &prefs.ncheck_type}

         NB: CONFLICT WITH DOCO - DOCO CLAIMS <=16 NAMES ONLY.
         NB: USING DOCO CONVENTION (<=16)
         NB: ENSURING AT LEAST ONE ARG. IS PRESENT (IF NOT, OMIT THIS ELT)

         NB: THE LAST 6 KEYWORDS BELOW AREN'T IN THE PRINTED DOCO!
      -->
         <element name="CHECKIMAGE_TYPE" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="16">
                     <simpleType>
                        <restriction base="string">
                           <enumeration value="NONE"/>
                           <enumeration value="IDENTICAL"/>
                           <enumeration value="BACKGROUND"/>
                           <enumeration value="BACKGROUND_RMS"/>
                           <enumeration value="MINIBACKGROUND"/>
                           <enumeration value="MINIBACK_RMS"/>
                           <enumeration value="-BACKGROUND"/>
                           <enumeration value="FILTERED"/>
                           <enumeration value="OBJECTS"/>
                           <enumeration value="-OBJECTS"/>
                           <enumeration value="APERTURES"/>
                           <enumeration value="SEGMENTATION"/>
                           <enumeration value="ASSOC"/>
                           <enumeration value="-PSF_PROTOS"/>
                           <enumeration value="PSF_PROTOS"/>
                           <enumeration value="-PC_CONVPROTOS"/>
                           <enumeration value="PC_CONVPROTOS"/>
                           <enumeration value="PC_PROTOS"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>

      <!-- ================================================================
         {"CLEAN", P_BOOL, &prefs.clean_flag}
      -->
         <element name="CLEAN" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" type="boolean"
                     minOccurs="1" maxOccurs="1">
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"CLEAN_PARAM", P_FLOAT, &prefs.clean_param, 0,0, 0.1,10.0},
      -->
         <element name="CLEAN_PARAM" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="1">
                     <simpleType>
                        <restriction base="double">
                           <minInclusive value="0.1"/>
                           <maxInclusive value="10.0"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"DEBLEND_MINCONT", P_FLOAT, &prefs.deblend_mincont, 0,0, 0.0,1.0}
      -->
         <element name="DEBLEND_MINCONT" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="1">
                     <simpleType>
                        <restriction base="double">
                           <minInclusive value="0.0"/>
                           <maxInclusive value="1.0"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"DEBLEND_NTHRESH", P_INT, &prefs.deblend_nthresh, 1,64}
      -->
         <element name="DEBLEND_NTHRESH" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="1">
                     <simpleType>
                        <restriction base="positiveInteger">
                           <maxInclusive value="64"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"DETECT_MINAREA", P_INT, &prefs.ext_minarea, 1,1000000}
      -->
         <element name="DETECT_MINAREA" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="1">
                     <simpleType>
                        <restriction base="positiveInteger">
                           <maxInclusive value="1000000"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"DETECT_THRESH", P_FLOATLIST, prefs.dthresh, 0,0, -BIG, BIG,
            {""}, 1, 2, &prefs.ndthresh}
      -->
         <element name="DETECT_THRESH" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="2">
                     <simpleType>
                        <restriction base="double">
                           <minInclusive value="-1.0E30"/>
                           <maxInclusive value="1.0E30"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
        {"DETECT_TYPE", P_KEY, &prefs.detect_type, 0,0, 0.0,0.0,
           {"CCD","PHOTO",""}}
      -->
         <element name="DETECT_TYPE" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="1">
                     <simpleType>
                        <restriction base="string">
                           <enumeration value="CCD"/>
                           <enumeration value="PHOTO"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"FILTER", P_BOOL, &prefs.filter_flag}
      -->
         <element name="FILTER" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" type="boolean"
                        minOccurs="1" maxOccurs="1"/>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"FILTER_NAME", P_STRING, prefs.filter_name}
      -->
         <element name="FILTER_NAME" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" type="string"
                     minOccurs="1" maxOccurs="1"/>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
        {"FILTER_THRESH", P_FLOATLIST, prefs.filter_thresh, 0,0,-BIG,BIG,
           {""}, 0, 2, &prefs.nfilter_thresh}

         NB: QUERY? DO WE NEED BOTH OF THESE IF PRESENT AT ALL?
         NB: ENSURING AT LEAST ONE ARG. IS PRESENT (IF NOT, OMIT THIS ELT)
      -->
         <element name="FILTER_THRESH" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="2">
                     <simpleType>
                        <restriction base="double">
                           <minInclusive value="-1.0E30"/>
                           <maxInclusive value="1.0E30"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"FITS_UNSIGNED", P_BOOL, &prefs.fitsunsigned_flag}
      -->
         <element name="FITS_UNSIGNED" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" type="boolean"
                     minOccurs="1" maxOccurs="1"/>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
        {"FLAG_IMAGE", P_STRINGLIST, prefs.fimage_name, 0,0,0.0,0.0,
            {""}, 0, MAXFLAG, &prefs.nfimage_name} /*MAXFLAG = 4*/

         NB: ENSURING AT LEAST ONE ARG. IS PRESENT (IF NOT, OMIT THIS ELT)
      -->
         <element name="FLAG_IMAGE" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" type="string"
                     minOccurs="1" maxOccurs="4"/>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"FLAG_TYPE",  P_KEYLIST, prefs.flag_type, 0,0, 0.0,0.0,
            {"OR","AND","MIN", "MAX", "MOST",""}, 0, MAXFLAG, &idummy}
      -->
         <element name="FLAG_TYPE" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="1">
                     <simpleType>
                        <restriction base="string">
                           <enumeration value="OR"/>
                           <enumeration value="AND"/>
                           <enumeration value="MIN"/>
                           <enumeration value="MAX"/>
                           <enumeration value="MOST"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"GAIN", P_FLOAT, &prefs.gain, 0,0, 0.0, 1e+30}
      -->
         <element name="GAIN" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="1">
                     <simpleType>
                        <restriction base="double">
                           <minInclusive value="0.0"/>
                           <maxInclusive value="1.0E30"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"INTERP_MAXXLAG", P_INTLIST, prefs.interp_xtimeout, 1,1000000,
            0.0,0.0, {""}, 1, 2, &prefs.ninterp_xtimeout}
      -->
         <element name="INTERP_MAXXLAG" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="2">
                     <simpleType>
                        <restriction base="nonNegativeInteger">
                           <maxInclusive value="1000000"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"INTERP_MAXYLAG", P_INTLIST, prefs.interp_ytimeout, 1,1000000,
            0.0,0.0, {""}, 1, 2, &prefs.ninterp_ytimeout}
      -->
         <element name="INTERP_MAXYLAG" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="2">
                     <simpleType>
                        <restriction base="nonNegativeInteger">
                           <maxInclusive value="1000000"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"INTERP_TYPE", P_KEYLIST, prefs.interp_type, 0,0, 0.0,0.0,
            {"NONE","VAR_ONLY","ALL",""}, 1, 2, &prefs.ninterp_type}
      -->
         <element name="INTERP_TYPE" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="2">
                     <simpleType>
                        <restriction base="string">
                           <enumeration value="NONE"/>
                           <enumeration value="VAR_ONLY"/>
                           <enumeration value="ALL"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"MAG_GAMMA", P_FLOAT, &prefs.mag_gamma, 0,0, 1e-10,1e+30}
      -->
         <element name="MAG_GAMMA" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="1">
                     <simpleType>
                        <restriction base="double">
                           <minInclusive value="1.0E-10"/>
                           <maxInclusive value="1.0E30"/>
                        </restriction>
                     </simpleType>
                  </element>
               </sequence>
            </complexType>
         </element>


      <!-- ================================================================
         {"MAG_ZEROPOINT", P_FLOAT, &prefs.mag_zeropoint, 0,0, -100.0, 100.0}
      -->
         <element name="MAG_ZEROPOINT" minOccurs="0" maxOccurs="1">
            <complexType>
               <sequence>
                  <element name="arg" minOccurs="1" maxOccurs="1">
                     <simpleType>
                        <restriction base="double">
                           <minInclusive value="-100.0"/>
                           <maxInclusive value="100.0"/>
                        </restriction>
                     </simpleType>
                  </element>
