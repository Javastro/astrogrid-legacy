<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../maven-base/xsd/maven-project.xsd">
    <!--will eventually extend from /base when Maven RC3 installed-->
    <extend>${basedir}/../maven-base/project.xml</extend>
    <currentVersion>${astrogrid.slinger.version}</currentVersion>
    <id>astrogrid-slinger</id>
    <name>Astrogrid Slinger Package</name>
    <description> Astrogrid Source/Target connecting package</description>
    <shortDescription>AstroGrid Slinger</shortDescription>


    <dependencies>
     <dependency>
         <artifactId>astrogrid-common</artifactId>
         <groupId>org.astrogrid</groupId>
         <version>${astrogrid.common.version}</version>
         <properties>
             <war.bundle>true</war.bundle>
         </properties>
     </dependency>
        
    <!-- UI stuff for viewers -->
    <dependency>
      <artifactId>astrogrid-ui</artifactId>
      <groupId>org.astrogrid</groupId>
        <version>${astrogrid.ui.version}</version>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    
    <!-- JavaMail for TargetIndicators -->
    <dependency>
      <id>JavaMail</id>
      <version>UNKNOWN</version>
      <jar>JavaMail.jar</jar>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>

   <!-- for the FTP Client -->
   <dependency>
      <artifactId>commons-net</artifactId>
      <groupId>commons-net</groupId>
      <version>1.1.0</version>
      <properties>
          <war.bundle>true</war.bundle>
      </properties>
   </dependency>

    <!-- For SRB -->
    <dependency>
      <artifactId>jargon</artifactId>
      <groupId>gsi</groupId>
      <version>1.4.6</version>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>

   <!-- Grid Service Interface classes? For jargon for SRB access above -->
   <dependency>
      <artifactId>GSI</artifactId>
      <groupId>gsi</groupId>
      <jar>gsi-classes.jar</jar>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
   </dependency>

    <!-- MySpace store clients -->
    <dependency>
      <artifactId>astrogrid-store-client</artifactId>
      <groupId>org.astrogrid</groupId>
        <version>${astrogrid.mySpace.version}</version>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>

   <!-- for the myspace storefile -->
   <dependency>
      <groupId>axis</groupId>
      <artifactId>axis</artifactId>
      <version>1.1</version>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
   </dependency>
   <dependency>
      <groupId>jaxrpc-api</groupId>
      <artifactId>jaxrpc-api</artifactId>
      <version>1.0.3</version>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
   </dependency>
   <dependency>
      <groupId>wsdl4j</groupId>
      <artifactId>wsdl4j</artifactId>
      <version>1.4</version>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
   </dependency>
   <dependency>
      <groupId>jax-qname</groupId>
      <artifactId>jax-qname</artifactId>
      <jar>jax-qname.jar</jar>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
   </dependency>
    <dependency>
       <id>saaj</id>
       <version>1.0</version>
       <jar>saaj.jar</jar>
       <properties>
         <war.bundle>true</war.bundle>
       </properties>
    </dependency>


      <!-- for VoSpaceResolver -->
     <dependency>
         <artifactId>astrogrid-registry-client-lite</artifactId>
      <groupId>org.astrogrid</groupId>
         <version>${astrogrid.registry.version}</version>
         <properties>
             <war.bundle>true</war.bundle>
         </properties>
     </dependency>
     <dependency>
         <artifactId>astrogrid-registry-common</artifactId>
      <groupId>org.astrogrid</groupId>
         <version>${astrogrid.registry.version}</version>
         <properties>
             <war.bundle>true</war.bundle>
         </properties>
     </dependency>
    
        <!-- The Community common components.-->
        <dependency>
            <groupId>org.astrogrid</groupId>
            <artifactId>astrogrid-community-common</artifactId>
         <version>${astrogrid.community.version}</version>
            <type>jar</type>
            <url>http://www.astrogrid.org/</url>
        </dependency>
        <!--+
            | The Community client components.
            +-->
        <dependency>
            <groupId>org.astrogrid</groupId>
            <artifactId>astrogrid-community-client</artifactId>
          <version>${astrogrid.community.version}</version>
            <type>jar</type>
            <url>http://www.astrogrid.org/</url>
        </dependency>
        <!--+
            | The Community resolver components.
            +-->
        <dependency>
            <groupId>org.astrogrid</groupId>
            <artifactId>astrogrid-community-resolver</artifactId>
           <version>${astrogrid.community.version}</version>
            <type>jar</type>
            <url>http://www.astrogrid.org/</url>
        </dependency>

        <!--+
            | The JXpath library - required for Registry delegate.
            +-->
       <dependency>
         <artifactId>commons-jxpath</artifactId>
            <groupId>commons-jxpath</groupId>
            <version>1.1</version>
            <type>jar</type>
            <url></url>
        </dependency>
        <!--+
            | The Jakarta Regexp library - required for Registry delegate.
            +-->
      <dependency>
         <artifactId>jakarta-regexp</artifactId>
            <groupId>jakarta-regexp</groupId>
            <version>1.3</version>
            <type>jar</type>
            <url></url>
        </dependency>
        <!--+
            | The Castor XML library - required for Registry delegate.
            +-->
        <dependency>
            <groupId>castor</groupId>
            <artifactId>castor-xml</artifactId>
            <version>0.9.5</version>
            <jar>castor-0.9.5-xml.jar</jar>
            <type>jar</type>
            <url>http://www.castor.org/</url>
        </dependency>
        <!--+
            | The Xerces library that Castor relies on.
            +-->
        <dependency>
            <groupId>xerces</groupId>
            <artifactId>xerces</artifactId>
            <version>2.4.0</version>
            <type>jar</type>
            <url>http://xml.apache.org/</url>
        </dependency>
        <!--+
            | The XML library that Xerces relies on.
            +-->
        <dependency>
            <groupId>xml-apis</groupId>
            <artifactId>xml-apis</artifactId>
            <version>1.0.b2</version>
            <type>jar</type>
            <url>http://xml.apache.org/</url>
        </dependency>

        <dependency>
            <artifactId>junit</artifactId>
            <groupId>junit</groupId>
            <version>3.8</version>
            <properties>
                <war.bundle>false</war.bundle>
            </properties>
        </dependency>

    <!-- servlet support (for compiling against - no need for deployment war as servlet container should have it) -->
    <dependency>
      <id>servletapi</id>
      <version>2.3</version>
      <properties>
        <war.bundle>false</war.bundle>
      </properties>
    </dependency>

      <!-- for logging -->
        <dependency>
            <artifactId>log4j</artifactId>
            <groupId>log4j</groupId>
            <version>1.2.4</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>
        <dependency>
            <artifactId>commons-logging</artifactId>
            <groupId>commons-logging</groupId>
            <version>1.0.3</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>
        <dependency>
            <artifactId>commons-discovery</artifactId>
            <groupId>commons-discovery</groupId>
            <version>20030211.213356</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>




    </dependencies>
