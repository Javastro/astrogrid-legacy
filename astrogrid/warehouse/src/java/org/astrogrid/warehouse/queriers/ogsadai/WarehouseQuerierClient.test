java -cp ${TESTCLASSPATH} org.astrogrid.warehouse.queriers.ogsadai.WarehouseQuerierClient /export/home/ogsa-dai-tomcat/conf/AstroGridConfig.properties "SELECT * from first where decl &gt; 59.9" 

