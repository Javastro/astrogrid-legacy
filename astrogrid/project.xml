<?xml version="1.0" encoding="UTF-8"?>
<!-- $Id: project.xml,v 1.13 2004/12/08 15:03:40 jdt Exp $ -->
<project>
    <extend>${basedir}/maven-base/project.xml</extend>
    <id>astrogrid</id>
    <name>AstroGrid</name>
    <shortDescription>AstroGrid Projects</shortDescription>
    <siteDirectory>${astrogrid.docs.root}</siteDirectory>
    <build>
    	<sourceDirectory>${basedir}/monster/src</sourceDirectory>
    </build>
    
    <dependencies/>
    <repository>
        <connection> scm:cvs:pserver:anoncvs@cvs.astrogrid.org:/devel:astrogrid 
            </connection>
        <developerConnection> 
            scm:cvs:ext:${maven.username}@cvs.astrogrid.org:/devel:astrogrid 
            </developerConnection>
        <url> http://www.astrogrid.org/viewcvs/astrogrid/ </url>
    </repository>
    <reports>
        <report> maven-javadoc-plugin </report>
        <report> maven-changelog-plugin </report>
        <report> maven-developer-activity-plugin </report>
        <report> maven-file-activity-plugin </report>
        <report> maven-linkcheck-plugin </report>
        <report> maven-changes-plugin </report>
        <report> maven-jxr-plugin </report>        
    </reports>
</project>