<DatasetDescription xmlns='urn:astrogrid:schema:TableMetaDoc:v1'
                     xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'>

   <!-- Note that this is in TWO places - in the classes directory for deployment, and
   also the package with SampleStarsMetaServer in, for unit tests -->
<Catalog>
   <Name>SampleStars</Name>
   <Table ID="SampleStars">
      <Name>SampleStars</Name>
      <Description>A table of artifically placed stars.  There is a general spread of background
      stars, and a smiley face around where the pleidies are on the real sky</Description>
      <Column  ID="SampleStars.ID" indexed="false">
         <Name>ID</Name>
         <Datatype>int</Datatype>
         <Description>IDENTITY</Description>
         <UCD version='1'>ID_CATALOG</UCD>
         <UCD version='1+'>meta.id</UCD>
      </Column>
      <Column  ID="SampleStars.NAME" indexed="false">
         <Name>Name</Name>
         <Datatype>string</Datatype>
         <Description>Made up name</Description>
         <UCD version='1'> </UCD>
         <UCD version='1+'> </UCD>
      </Column>
      <Column  ID="SampleStars.RA" indexed="false">
         <Name>RA</Name>
         <Datatype>float</Datatype>
         <Description>Right Ascension (left-rightness)</Description>
         <Units>deg</Units>
         <UCD version='1'>POS_EQ_RA_MAIN</UCD>
         <UCD version='1+'>pos.eq.ra</UCD>
         <ErrorColumn> </ErrorColumn>
         <SkyPolarCoord>RA</SkyPolarCoord>
      </Column>
      <Column  ID="SampleStars.DEC" indexed="false">
         <Name>DEC</Name>
         <Datatype>float</Datatype>
         <Description>Declination (up-downness)</Description>
         <Units>deg</Units>
         <UCD version='1'>POS_EQ_DEC_MAIN</UCD>
         <UCD version='1+'>pos.eq.dec</UCD>
         <ErrorColumn> </ErrorColumn>
         <SkyPolarCoord>DEC</SkyPolarCoord>
      </Column>
      <Column  ID="SampleStars.MAG" indexed="false">
         <Name>Mag</Name>
         <Datatype>float</Datatype>
         <Description>Magnitude at some arbitrary wavelength</Description>
         <Units>mag</Units>
         <UCD version='1'>PHOT_MAG</UCD>
         <UCD version='1+'> </UCD>
         <ErrorColumn> </ErrorColumn>
      </Column>
      <Column  ID="SampleStars.FLAG" indexed="false">
         <Name>Flag</Name>
         <Datatype>boolean</Datatype>
         <Description>Test field of boolean type</Description>
         <Units></Units>
         <UCD version='1'></UCD>
         <UCD version='1+'> </UCD>
         <ErrorColumn> </ErrorColumn>
      </Column>
   </Table>
   <Table ID="SampleGalaxies">
      <Name>SampleGalaxies</Name>
      <Description>A table of artifically placed galaxies</Description>
      <Column  ID="SampleGalaxies.ID" indexed="false">
         <Name>ID</Name>
         <Datatype>int</Datatype>
         <Description>IDENTITY</Description>
         <UCD version='1'>ID_CATALOG</UCD>
         <UCD version='1+'>meta.id</UCD>
      </Column>
      <Column  ID="SampleGalaxies.RA" indexed="false">
         <Name>RA</Name>
         <Datatype>float</Datatype>
         <Description>Right Ascension (left-rightness)</Description>
         <Units>deg</Units>
         <UCD version='1'>POS_EQ_RA_MAIN</UCD>
         <UCD version='1+'>pos.eq.ra</UCD>
         <ErrorColumn> </ErrorColumn>
         <SkyPolarCoord>RA</SkyPolarCoord>
      </Column>
      <Column  ID="SampleGalaxies.DEC" indexed="false">
         <Name>DEC</Name>
         <Datatype>float</Datatype>
         <Description>Declination (up-downness)</Description>
         <Units>deg</Units>
         <UCD version='1'>POS_EQ_DEC_MAIN</UCD>
         <UCD version='1+'>pos.eq.dec</UCD>
         <ErrorColumn> </ErrorColumn>
         <SkyPolarCoord>DEC</SkyPolarCoord>
      </Column>
      <Column  ID="SampleGalaxies.SHAPE" indexed="false">
         <Name>Shape</Name>
         <Datatype>string</Datatype>
         <Description>Rough shape (type) of galaxy; eg elliptical, spherical, barred etc </Description>
         <UCD version='1'>MORPH</UCD>
         <UCD version='1+'>src.morph</UCD>
      </Column>
   </Table>
   <Table ID="Plates">
      <Name>Plates</Name>
      <Description>Details of the plates behind the observations</Description>
      <Column  ID="Plates.ID" indexed="false">
         <Name>ID</Name>
         <Datatype>int</Datatype>
         <Description>IDENTITY</Description>
         <UCD version='1'>ID_CATALOG</UCD>
         <UCD version='1+'>meta.id</UCD>
      </Column>
      <Column ID="Plates.CENTERRA" indexed="false">
         <Name>CENTERRA</Name>
         <Datatype>float</Datatype>
         <Description>Right Ascension (left-rightness)</Description>
         <Units>deg</Units>
         <UCD version='1'>POS_EQ_RA_MAIN</UCD>
         <UCD version='1+'>pos.eq.ra</UCD>
         <ErrorColumn> </ErrorColumn>
      </Column>
      <Column ID="Plates.CENTERDEC" indexed="false" >
         <Name>CENTERDEC</Name>
         <Datatype>float</Datatype>
         <Description>Declination (up-downness)</Description>
         <Units>deg</Units>
         <UCD version='1'>POS_EQ_DEC_MAIN</UCD>
         <UCD version='1+'>pos.eq.dec</UCD>
         <ErrorColumn> </ErrorColumn>
      </Column>
      <Column ID="Plates.ObsTime" indexed="false">
         <Name>ObsTime</Name>
         <Datatype>dateTime</Datatype>
         <Description>Date plate was taken</Description>
         <UCD version='1'></UCD>
      </Column>
   </Table>
</Catalog>
</DatasetDescription>

