         <Units>mag</Units>
         <UCD version='1'>PHOT_MAG</UCD>
         <UCD version='1+'> </UCD>
         <ErrorColumn> </ErrorColumn>
      </Column>
      <Column  ID="FLAG" indexed="false">
         <Name>ColName_Flag</Name>
         <Datatype>boolean</Datatype>
         <Description>Test field of boolean type</Description>
         <Units></Units>
         <UCD version='1'></UCD>
         <UCD version='1+'> </UCD>
         <ErrorColumn> </ErrorColumn>
      </Column>
   </Table>
</Catalog>
</DatasetDescription>

