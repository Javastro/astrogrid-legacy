<?xml version="1.0" encoding="UTF-8"?>
<project>
    <extend>${basedir}/../../maven-base/project.xml</extend>
        <id>astrogrid-pal-server</id>
        <name>Astrogrid Server Library</name>
  <currentVersion>${astrogrid.pal.version}</currentVersion>
  <description>A set of classes to manage queries made through plugins on data</description>
  <shortDescription>AstroGrid Server Library</shortDescription>

   <licenses>
      <license>
         <name>The AstroGrid Software License, Version 1.2</name>
         <url>http://www.astrogrid.org</url>
         <distribution>repo</distribution>
      </license>
   </licenses>

  <dependencies>
    <!-- astrogrid -specific deps -->
    <dependency>
      <id>astrogrid-common</id>
      <groupId>org.astrogrid</groupId>
      <version>${astrogrid.common.version}</version>
    </dependency>

    <dependency>
      <id>astrogrid-slinger-api</id>
      <groupId>org.astrogrid</groupId>
      <version>${astrogrid.slinger.version}</version>
    </dependency>

    <dependency>
      <id>astrogrid-slinger-xml</id>
      <groupId>org.astrogrid</groupId>
      <version>${astrogrid.slinger.version}</version>
    </dependency>

    <dependency>
      <id>astrogrid-status-api</id>
      <groupId>org.astrogrid</groupId>
      <version>${astrogrid.status.version}</version>
    </dependency>

      <dependency>
         <artifactId>astrogrid-pal-sky</artifactId>
         <groupId>org.astrogrid</groupId>
         <version>${astrogrid.pal.version}</version>
      </dependency>

      <dependency>
         <artifactId>astrogrid-pal-units</artifactId>
         <groupId>org.astrogrid</groupId>
         <version>${astrogrid.pal.version}</version>
      </dependency>

      <dependency>
         <artifactId>astrogrid-pal-api</artifactId>
         <groupId>org.astrogrid</groupId>
         <version>${astrogrid.pal.version}</version>
      </dependency>

      <dependency>
         <artifactId>astrogrid-pal-ucd</artifactId>
         <groupId>org.astrogrid</groupId>
         <version>${astrogrid.pal.version}</version>
      </dependency>

      <dependency>
         <artifactId>astrogrid-pal-query</artifactId>
         <groupId>org.astrogrid</groupId>
         <version>${astrogrid.pal.version}</version>
      </dependency>

      <dependency>
         <artifactId>astrogrid-pal-fits</artifactId>
         <groupId>org.astrogrid</groupId>
         <version>${astrogrid.pal.version}</version>
      </dependency>

    <!-- exist db for fits plugin? -->
    <dependency>
      <groupId>org.astrogrid</groupId>
      <artifactId>astrogrid-exist-server</artifactId>
      <version>${astrogrid.eXist.version}</version>
    </dependency>

    <!-- more standard deps -->

    <!-- servlet support (for compiling against - no need for deployment war as servlet container should have it) -->
    <dependency>
      <id>servletapi</id>
      <version>2.3</version>
    </dependency>

    <!-- cea -->
    <dependency>
      <artifactId>astrogrid-cea-server</artifactId>
      <groupId>org.astrogrid</groupId>
      <version>${astrogrid.applications.version}</version>
    </dependency>
    <dependency>
      <artifactId>astrogrid-workflow-objects</artifactId>
      <groupId>org.astrogrid</groupId>
      <version>${astrogrid.workflowobjects.version}</version>
    </dependency>
    <dependency>
      <id>picocontainer</id>
      <version>1.0</version>
    </dependency>
    <dependency>
      <id>concurrent</id>
      <version>1.3.1</version>
    </dependency>

    <!-- registry for resolving IVORNs etc and pushing metadata -->
    <dependency>
      <artifactId>astrogrid-registry-client-lite</artifactId>
      <groupId>org.astrogrid</groupId>
      <version>${astrogrid.registry.version}</version>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
   
    <dependency>
      <artifactId>astrogrid-registry-common</artifactId>
      <groupId>org.astrogrid</groupId>
      <version>${astrogrid.registry.version}</version>
      <properties>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>


      <!-- axis, and the jars it relies upon -->
    <dependency>
      <id>axis</id>
      <version>1.1</version>
    </dependency>

    <dependency>
      <id>wsdl4j</id>
      <version>1.0</version>
    </dependency>

     <dependency>
        <id>saaj</id>
        <version>1.0</version>
        <jar>saaj.jar</jar>
     </dependency>

    <dependency>
      <id>commons-logging</id>
      <version>1.0.3</version>
    </dependency>

    <dependency>
      <id>commons-discovery</id>
      <version>20030211.213356</version>
    </dependency>

    <dependency>
      <id>jaxrpc-api</id>
      <version>1.0.3</version>
    </dependency>

    <dependency>
      <id>xml-apis</id>
      <version>1.0.b2</version>
    </dependency>

    <dependency>
      <id>hsqldb</id>
      <version>1.7.1</version>
    </dependency>

    <dependency>
      <artifactId>stil</artifactId>
      <groupId>starlink</groupId>
      <version>2.0-3</version>
    </dependency>

    <!-- castor, and what it needs -->
    <dependency>
      <id>castor</id>
      <version>0.9.5</version>
      <jar>castor-0.9.5-xml.jar</jar>
    </dependency>

    <dependency>
        <id>xerces</id>
        <version>2.4.0</version>
    </dependency>

    <dependency>
      <id>regexp</id>
      <version>1.3</version>
    </dependency>
    <dependency>
      <groupId>oro</groupId>
      <artifactId>oro</artifactId>
      <version>2.0.7</version>
    </dependency>
    <dependency>
      <artifactId>commons-jxpath</artifactId>
      <groupId>commons-jxpath</groupId>
      <version>1.1</version>
    </dependency>
    <dependency>
      <groupId>commons-collections</groupId>
      <artifactId>commons-collections</artifactId>
      <version>2.1</version>
    </dependency>
    <dependency>
      <groupId>commons-beanutils</groupId>
      <artifactId>commons-beanutils</artifactId>
      <version>1.6.1</version>
    </dependency>

    <!-- testing -->
    <dependency>
      <id>junit</id>
      <version>3.8</version>
    </dependency>

    <dependency>
        <id>xmlunit</id>
        <version>1.0</version>
    </dependency>
    </dependencies>


</project>
