<?xml version="1.0"?>
<!-- extension maven script -->
<project xmlns:j="jelly:core" xmlns:maven="jelly:maven" xmlns:jsl="jelly:jsl" xmlns:jxml="jelly:xml" >

	<!--  indicates we're building the workbench -->
	  	<goal name="setup">
			<j:set var="ifBuildingASR" value="java.lang.Object" />
			<j:set var="ifBuildingACR" value="java.lang.Object" />			
		</goal>
	
	<!--  generates an anouncement.txt after all other things have been built -->
	<postGoal name="dist:build-bin">
		<attainGoal name="announcement:generate" />
	</postGoal>


</project>

