<!-- sesame -->		
		<component name="sesame"
			implementationClass="org.astrogrid.desktop.modules.cds.SesameImpl"
			interfaceClass="org.astrogrid.acr.cds.Sesame">
			<description>This XML Web Service resolves a name which is present in , Simbad and/or NED and/or VizieR.</description>
	 <property>
		<key>icon</key>
		<value>simbad_icon.gif</value>
	</property>
			<method name="sesame">
				<description>Resolve a name</description>
			<property>
		<key>icon</key>
		<value>info_obj.gif</value>
	</property>	
	<property>
		<key>system.ui.menu.entry</key>
		<value>Resolve a Name</value>
	</property>		
		<return>
			<description>format depending on the result type parameter</description>
		</return>
		<parameter name="name">
			<description>name to resolve - e.g. m31</description>
		</parameter>
		<parameter name="resultType">
			<description><![CDATA[
				<ul>
					<li>u - usual</li>
					<li>H - html</li>
					<li>x - XML - (XSD at http://vizier.u-strasbg.fr/xml/sesame_1.xsd) </li>
					<li>p - plain</li>
					<li>i (for all identiiers) can be added to H or x</li>				
			   </ul>
			]]></description>
		</parameter>		
			</method>
			<method name="sesameChooseService">
				<description>Resolve a name</description>
			<property>
		<key>icon</key>
		<value>info_obj.gif</value>
	</property>	
	<property>
		<key>system.ui.menu.entry</key>
		<value>Resolve a name in selected services</value>
	</property>		
		<return>
			<description>format depending on the result type parameter</description>
		</return>
		<parameter name="name">
			<description>name to resolve - e.g. m31</description>
		</parameter>
		<parameter name="resultType">
			<description><![CDATA[
				<ul>
					<li>u - usual</li>
					<li>H - html</li>
					<li>x - XML - (XSD at http://vizier.u-strasbg.fr/xml/sesame_1.xsd) </li>
					<li>p - plain</li>
					<li>i (for all identiiers) can be added to H or x</li>				
			   </ul>
			]]></description>			
		</parameter>		
		<parameter name="all"><description>'true' if all identifiers wanted. otherwise 'false'</description>			
			<property><key>system.xmlrpc.type</key><value>boolean</value></property>
		</parameter>
		<parameter name="service"><description><![CDATA[
		<ul>
	<li> S=Simbad</li>
    <li>N=NED </li>
	<li>V=VizieR</li>
     <li>A=all</li>
      </ul>
        (examples : S to query in Simbad, NS to query in Ned if not found in Simbad,
        NSA to query in Ned and Simbad, A to query in Ned, Simbad and VizieR, ...)
		
		]]></description>
		</parameter>
			</method>									
	</component>	
			
