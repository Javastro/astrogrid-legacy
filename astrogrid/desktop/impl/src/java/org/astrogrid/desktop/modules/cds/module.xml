	<!-- coordinate -->
	<component name="coordinate"
		implementationClass="org.astrogrid.desktop.modules.cds.CoordinateImpl"
			interfaceClass="org.astrogrid.acr.cds.Coordinate">
			<property>
		<key>icon</key>
		<value>event_prop_arrow_obj.gif</value>
	</property>			
			<description>The Astronomical Coordinate XML Web Service</description>
			<method name="convert">
				<description>convert a coordinate</description>
			<property>
		<key>icon</key>
		<value>event_prop_arrow_obj.gif</value>
	</property>	
	<property>
		<key>system.ui.menu.entry</key>
		<value>Convert a coordinate</value>
	</property>				
		<return><description>a string like 03 45 14.3838 +47 58 07.990 (J2000.0)</description></return>
		<parameter name="x"><description>e.g. 10.0</description>
			<property><key>system.xmlrpc.type</key><value>int</value></property>
			</parameter>
		<parameter name="y"><description>e.g. 15.0</description>
			<property><key>system.xmlrpc.type</key><value>int</value></property>
			</parameter>
		<parameter name="z"><description>e.g. 20.0</description>
			<property><key>system.xmlrpc.type</key><value>int</value></property>
			</parameter>
		<parameter name="precision"><description>(0=NONE, 1=DEG, 3=ARCMIN, 5=ARCSEC, 8=MAS)</description>
			</parameter>		
			</method>
			<method name="convertL">
				<description>convert a long-latt coordinate</description>
			<property>
		<key>icon</key>
		<value>event_prop_arrow_obj.gif</value>
	</property>	
	<property>
		<key>system.ui.menu.entry</key>
		<value>Convert a Long.-Latt. coordinate</value>
	</property>				
		<return><description>a String like 04 21 34. +53 32.5 (J2000.0)</description></return>
		<parameter name="lon"><description>e.g. 12.0</description>
			<property><key>system.xmlrpc.type</key><value>int</value></property>
			</parameter>
		<parameter name="lat"><descriiption>e.g. 45.0</descriiption>
			<property><key>system.xmlrpc.type</key><value>int</value></property>
			</parameter>
		<parameter name="precision"><description>(0=NONE, 1=DEG, 3=ARCMIN, 5=ARCSEC, 8=MAS)</description>
			</parameter>		
			</method>
			
			<method name="convertE">
				<description>Convert a coordinate considering equinox.</description>
			<property>
		<key>icon</key>
		<value>event_prop_arrow_obj.gif</value>
	</property>	
	<property>
		<key>system.ui.menu.entry</key>
		<value>Convert a coordinate considering Equinox</value>
	</property>				
		<return><description>a string like150.4806267 -05.3873952 (Gal)</description></return>
		<parameter name="frame1"><description>(1=FK4, 2=GAL, 3=SGAL, 4=ECL, 5=FK5, 6=ICRS)</description>
			<property><key>system.xmlrpc.type</key><value>int</value></property>
			</parameter>
		<parameter name="frame2"><description>(1=FK4, 2=GAL, 3=SGAL, 4=ECL, 5=FK5, 6=ICRS)</description>
			<property><key>system.xmlrpc.type</key><value>int</value></property>
			</parameter>			
		<parameter name="x"><description>e.g. 10.0</description>
			<property><key>system.xmlrpc.type</key><value>int</value></property>
			</parameter>
		<parameter name="y"><description>e.g. 15.0</description>
			<property><key>system.xmlrpc.type</key><value>int</value></property>
			</parameter>
		<parameter name="z"><description>e.g. 20.0</description>
			<property><key>system.xmlrpc.type</key><value>int</value></property>
			</parameter>
		<parameter name="precision"><description>(0=NONE, 1=DEG, 3=ARCMIN, 5=ARCSEC, 8=MAS)</description>
			</parameter>	
		<parameter name="equinox1"><description>Julian Years or Besselian, unused for GAL, SGAL, ICRS</description></parameter>
		<parameter name="equinox2"><description>Julian Years or Besselian, unused for GAL, SGAL, ICRS</description></parameter>			
			</method>
			<method name="convertLE">
				<description>Convert a longittude-lattitude coordinate considering equinox.</description>
			<property>
		<key>icon</key>
		<value>event_prop_arrow_obj.gif</value>
	</property>	
	<property>
		<key>system.ui.menu.entry</key>
		<value>Convert a long-latt coordinate considering Equinox</value>
	</property>				
		<return><description>a string like150.4806267 -05.3873952 (Gal)</description></return>
		<parameter name="frame1"><description>(1=FK4, 2=GAL, 3=SGAL, 4=ECL, 5=FK5, 6=ICRS)</description>
			<property><key>system.xmlrpc.type</key><value>int</value></property>
			</parameter>
		<parameter name="frame2"><description>(1=FK4, 2=GAL, 3=SGAL, 4=ECL, 5=FK5, 6=ICRS)</description>
			<property><key>system.xmlrpc.type</key><value>int</value></property>
			</parameter>			
		<parameter name="lon"><description>e.g. 12.0</description>
			<property><key>system.xmlrpc.type</key><value>int</value></property>
			</parameter>
		<parameter name="lat"><descriiption>e.g. 45.0</descriiption>
			<property><key>system.xmlrpc.type</key><value>int</value></property>
			</parameter>
		<parameter name="precision"><description>(0=NONE, 1=DEG, 3=ARCMIN, 5=ARCSEC, 8=MAS)</description>
			</parameter>	
		<parameter name="equinox1"><description>Julian Years or Besselian, unused for GAL, SGAL, ICRS</description></parameter>
		<parameter name="equinox2"><description>Julian Years or Besselian, unused for GAL, SGAL, ICRS</description></parameter>			
			</method>										
		</component>
		  
<!-- ucd resolver. -->
	<component name="ucd"
		implementationClass="org.astrogrid.desktop.modules.cds.UCDImpl"
		interfaceClass="org.astrogrid.acr.cds.UCD">
		<description>This XML Web Service provides a few methods to ease manipulation of the
Unified Content Descriptors (UCD).</description>
			<method name="UCDList"><description>List all UCD1</description>
					<property>
			<key>system.ui.menu.entry</key>
			<value>List all UCD1</value>
		</property>	
			<property>
		<key>icon</key>
		<value>document.gif</value>
		</property>		
				<return><description>html document containing all ucd1</description></return>
			</method>
			<method name="resolveUCD"><description>Resolve a UCD1 (won't work with UCD1+)</description>
					<property>
			<key>system.ui.menu.entry</key>
			<value>Resolve a UCD1</value>
		</property>	
			<property>
		<key>icon</key>
		<value>info_obj.gif</value>
		</property>		
				<return><description>sentance corresponding to the UCD1</description></return>
				<parameter name="ucd"><description>the UCD1 to resolve (e.g. PHOT_JHN_V )</description></parameter>
			</method>			
			
			<method name="UCDofCatalog"><description>List UCD1 contained in a given catalogue</description>
					<property>
			<key>system.ui.menu.entry</key>
			<value>List UCD1 for a catalogue</value>
		</property>	
			<property>
		<key>icon</key>
		<value>flatLayout.gif</value>
		</property>		
				<return><description>list of UCD1 (in raw text) contained in the given catalogue</description></return>
				<parameter name="catalog_designation"><description>designes the catalogue (example : I/239) </description></parameter>
			</method>			
	
			<method name="translate"><description>makes the translation of old-style UCD1 into the newer UCD1+ easier:</description>
					<property>
			<key>system.ui.menu.entry</key>
			<value>Translate from UCD1 to UCD1+</value>
		</property>	
			<property>
		<key>icon</key>
		<value>event_prop_arrow_obj.gif</value>
		</property>		
				<return><description>
String ucd. This function returns the default UCD1+ corresponding to an old-style UCD1.				
					</description></return>
				<parameter name="ucd"><description>
The argument is a UCD1 (not UCD1+ !).
					 </description></parameter>
			</method>		
						
			<method name="upgrade"><description>Upgrade a UCD</description>
					<property>
			<key>system.ui.menu.entry</key>
			<value>Upgrade a UCD1+</value>
		</property>	
			<property>
		<key>icon</key>
		<value>event_prop_arrow_obj.gif</value>
		</property>		
				<return><description>
String ucd. This function returns a valid UCD1+ corresponding to a deprecated word.
                      It is useful when some reference words of the UCD1+ vocabulary are changed,
                      and ensures backward compatibility.					
					</description></return>
				<parameter name="ucd"><description>
a deprecated UCD1+ (word or combination).
                    Useful when the 'validate' method returns with code 2.
					 </description></parameter>
			</method>					
			
			<method name="validate"><description>Validate a UCD</description>
					<property>
			<key>system.ui.menu.entry</key>
			<value>Validate a UCD1+</value>
		</property>	
			<property>
		<key>icon</key>
		<value>complete_status.gif</value>
		</property>		
				<return><description><![CDATA[
This function checks that a UCD is well-formed
<p/>
The first word of the string is an error code, possibly followed by an explanation of the error. 
A return value of 0 indicates no error (valid UCD).
<p/>
The error-code results from the combination (logical OR) of the following values:
<ul>
    <li>1: warning indicating use of non-standard namespace (not ivoa:)</li>
    <li> 2: use of deprecated word</li>
    <li> 4: use of non-existing word</li>
    <li> 8: syntax error (extra space or unallowed character) </li>
</ul>			
					]]></description></return>
				<parameter name="ucd"><description>
(e.g. ivoa:phot.mag;em.opt.B)
					 </description></parameter>
			</method>		
			
	<method name="explain"><description>Return description of a ucd</description>
					<property>
			<key>system.ui.menu.entry</key>
			<value>Explain a UCD1+</value>
		</property>	
			<property>
		<key>icon</key>
		<value>info_obj.gif</value>
		</property>		
				<return><description>
 this function gives a human-readable explanation for a UCD composed of one or several words			
					</description></return>
				<parameter name="ucd"><description>
 (e.g. ivoa:phot.mag;em.opt.B)
					 </description></parameter>
			</method>			
			
	<method name="assign"><description>Find the UCD associatedwith a description</description>
					<property>
			<key>system.ui.menu.entry</key>
			<value>Find UCD1+</value>
		</property>	
			<property>
		<key>icon</key>
		<value>search.gif</value>
		</property>		
				<return><description>
This function returns the UCD1+ corresponding to the description		
					</description></return>
				<parameter name="description"><description>
Plain text description of a parameter to be described         
					 </description></parameter>
			</method>															
		</component>
				  
	  
