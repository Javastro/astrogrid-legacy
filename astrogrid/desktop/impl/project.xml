<?xml version="1.0" encoding="UTF-8"?>

<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <extend>${basedir}/../template/project-template.xml</extend>
    <pomVersion>1</pomVersion>
    <artifactId>vodesktop</artifactId>
    <name>VODesktop</name>
    <description>AstroGrid VODesktop</description>
    <shortDescription>VODesktop - Explore the Virtual Observatory</shortDescription>
     <siteAddress>terror.star.le.ac.uk</siteAddress>
      <siteDirectory>/software_docs/vodesktop-${pom.currentVersion}</siteDirectory>
    
    <repository>
        <connection>scm:cvs:ext:${maven.username}@cvs.astrogrid.org:/devel:astrogrid/desktop/impl</connection>
        <developerConnection>scm:cvs:ext:${maven.username}@cvs.astrogrid.org:/devel:astrogrid/desktop/impl</developerConnection>
        <url>http://www.astrogrid.org/viewcvs/astrogrid/desktop/impl/</url>
    </repository>
    <packageGroups>
        <packageGroup>
            <title>Framework</title>
            <packages>org.astrogrid.desktop.framework*:org.astrogrid:org.astrogrid.desktop:org.astrogrid.desktop.hivemind:org.astrogrid.desktop.protocol*:org.astrogrid.desktop.alternatives:org.astrogrid.desktop.modules.test</packages>
        </packageGroup>
        <packageGroup>
            <title>System</title>
            <packages>org.astrogrid.desktop.modules.system*:org.astrogrid.desktop.modules.auth</packages>
        </packageGroup>
        <packageGroup>
            <title>Virtual Observatory</title>
            <packages>org.astrogrid.desktop.modules.ag*:org.astrogrid.desktop.modules.cds*:org.astrogrid.desktop.modules.ivoa*:org.astrogrid.desktop.modules.vo*:org.astrogrid.desktop.modules.plastic:org.astrogrid.desktop.modules.util</packages>
        </packageGroup>
        <packageGroup>
            <title>UI and Dialogues</title>
            <packages>org.astrogrid.desktop.modules.dialogs*:org.astrogrid.desktop.modules.ui*:org.astrogrid.desktop.icons:org.astrogrid.desktop.modules.adqlEditor*</packages>
        </packageGroup>
    </packageGroups>
    <build>
        <sourceDirectory>src/java</sourceDirectory>
        <unitTestSourceDirectory>test/java</unitTestSourceDirectory>
        <unitTest>
            <includes>
               
                    <include>**/AllRepeatableTests.java</include>
                <!-- fallback..
                <include>**/AbsolutelyAllUnitTests.java</include>
                -->
            </includes>
            <resources>
                <resource>
                    <directory>${basedir}/test/java</directory>
                    <includes>
                        <include>**/*.properties</include>
                        <include>**/*.xml</include>
                        <include>**/*.xsl</include>
                        <include>**/*.dtd</include>
                        <include>**/*.xsd</include>
                        <include>**/*.vot</include>
                        <include>**/*.votable</include>                        
                        <include>**/test-resources/*</include>
                    </includes>
                    <filtering>false</filtering>
                </resource>
            </resources>
        </unitTest>
        <resources>
            <resource>
                <directory>${basedir}/src/java</directory>
                <includes>
                    <include>*.properties</include>
                    <include>**/hivedoc/*.*</include>
                    <include>**/*.xml</include>
                    <include>**/*.png</include>
                    <include>**/*.jpg</include>
                    <include>**/*.html</include>
                    <include>**/*.xsl</include>
                    <include>**/*.gif</include>
                    <include>**/*.dtd</include>
                    <include>**/*.xsd</include>
                    <include>**/*.list</include>
                    <!--  ucd list -->
                    <include>**/*.py</include>
                    <!-- script templates -->
                </includes>
                <filtering>false</filtering>
            </resource>
        </resources>
    </build>
    <!-- small set of reports to run when generating project docs
    -->
    <reports>
        <report>maven-changes-plugin</report>
        <report>maven-javadoc-plugin</report>
        <report>maven-jxr-plugin</report>
    </reports>
    <dependencies>
    <!-- 
    
    I'm using some addtional property tags to manage things. some are used within
    the build, and others are just for documentation.
    
    Used in build:
    dist.bundle - boolean - indicates whether to include this jar in the final app.
    strip - boolean - indicates whether this jar should be stripped / compressed
    testing.jar - boolean - indicates that this jar is justused for testing.
    
    Documentation:
    version-held - date - date that version was checked, newer version found, and decision to hold at this version was made.
    version-checked - date - date that version was checked (and no newer version found)
    part-of - indicates that this jar is a dependency of some other lib (and indicate required version)
    upgraded - date - date when version was checked, new version found, and upgraded to
     -->
