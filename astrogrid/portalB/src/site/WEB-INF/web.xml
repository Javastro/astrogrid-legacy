<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
	"http://java.sun.com/j2ee/dtds/web-app_2.2.dtd">

<web-app>

<servlet>
        <servlet-name>DataQuery</servlet-name>
        <servlet-class>
                org.astrogrid.portal.query.DataQueryServlet
        </servlet-class>
</servlet>

<servlet>
	<servlet-name>Registry</servlet-name>
	<servlet-class>
		org.astrogrid.portal.registry.RegistryServlet
	</servlet-class>
</servlet>

<servlet-mapping>
        <servlet-name>DataQuery</servlet-name>
        <url-pattern>/DataQuery</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>Registry</servlet-name>
	<url-pattern>/Registry</url-pattern>
</servlet-mapping>

</web-app>
