<xsp:page language="java"
	xmlns:xsp="http://apache.org/xsp"
	xmlns:xscript="http://apache.org/xsp/xscript/1.0"
	xmlns:soap="http://apache.org/xsp/soap/3.0"
	xmlns:ci="http://apache.org/cocoon/include/1.0"
	>

	<!-- Initialisation code here -->
	<xsp:structure>
	</xsp:structure>
	<!-- Class level code here -->
	<xsp:logic>
		<![CDATA[
		]]>
	</xsp:logic>
	<page>
		<!-- Request level code here -->
		<xsp:logic>
			<![CDATA[
			]]>
		</xsp:logic>
		<!-- Test the calculator services ... -->
		<test>
			<xsp:comment>SOAP call to Axis service</xsp:comment>
			<soap:call url="http://localhost:8080/axis/Calculator.jws">
				<calc:add xmlns:calc="astrogrid:soap/calc">
					<i1 type="xsd:int">5</i1>
					<i2 type="xsd:int">20</i2>
				</calc:add>
			</soap:call>
		</test>
		<test>
			<xsp:comment>SOAP call to Cocoon service</xsp:comment>
			<soap:call url="http://localhost:8080/cocoon/astrogrid/soap/service/calc">
				<calc:add xmlns:calc="astrogrid:soap/calc">
					<i1 type="xsd:int">5</i1>
					<i2 type="xsd:int">20</i2>
				</calc:add>
			</soap:call>
		</test>

		<wsdl>
			<xsp:comment>WSDL for the Axis service</xsp:comment>
			<ci:includexml>
				<ci:src>http://localhost:8080/axis/Calculator.jws?wsdl</ci:src>
			</ci:includexml>
		</wsdl>

	</page>
</xsp:page>
