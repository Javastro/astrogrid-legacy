<request>
	<!-- Who is asking -->
	<user>
		<user>frog</user>
		<community>xxxx</community>
	</user>
	<!-- What they want to match -->
	<query>
		<!-- Path of the matching items -->
		<path>//collection[@name='tables']</path>
		<!-- Type(s) of the matching items -->
		<types>
			<type>VO-TABLE</type>
			<type>AST-CONTAINER</type>
		</types>
	</query>
	<!-- What they want back -->
	<select>
		<filter>
			<!-- Type(s) of the matching items -->
			<types>
				<type>AST-CONTAINER</type>
			</types>
		</filter>
		<!-- The format of the response -->
		<format>AST-FORMAT-NESTED|AST-FORMAT-LIST</format>
	</select>
</request>
