<?xml version="1.0" encoding="UTF-8"?>
<VODescription 
   xmlns="http://www.ivoa.net/xml/VOResource/v0.9" 
   xmlns:cea="http://www.ivoa.net/xml/CEAService/v0.1" 
   xmlns:vr="http://www.ivoa.net/xml/VOResource/v0.9" 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Resource  updated="2004-11-20T15:34:22Z" status="active" xmlns="http://www.ivoa.net/xml/VOResource/v0.9" xsi:type="cea:CeaApplicationType">
        <Identifier>
            <AuthorityID>roe.ac.uk</AuthorityID>
            <ResourceKey>datacenter/SSA-Proxy/ceaApplication</ResourceKey>
        </Identifier>
        <Title>SSA - AstroGrid PAL</Title>
        <ShortName>SSA-PAL</ShortName>
        <Summary>
            <Description>SuperCOSMOS Science Archive Proxy</Description>
            <ReferenceURL>http://grendel12.roe.ac.uk:8080/pal-ssa-proxy/</ReferenceURL>
        </Summary>
        <Type>Other</Type>
        <Curation>
            <Publisher>
                <Title>Royal Observatory Edinburgh</Title>
            </Publisher>
            <Contact>
                <Name>Martin Hill</Name>
                <Email>mch@roe.ac.uk</Email>
            </Contact>
        </Curation>
        <cea:ApplicationDefinition xmlns:cea="http://www.ivoa.net/xml/CEAService/v0.1">
            <cea:Parameters>
                <cea:ParameterDefinition name="Query" type="ADQL">
                    <ceapd:UI_Name xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Query</ceapd:UI_Name>
                    <ceapd:UI_Description xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Astronomy Data Query Language that defines the search criteria</ceapd:UI_Description>
                    <ceapd:UCD xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                    <ceapd:DefaultValue xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                    <ceapd:Units xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                </cea:ParameterDefinition>
                <cea:ParameterDefinition name="Result" type="text">
                    <ceapd:UI_Name xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Result</ceapd:UI_Name>
                    <ceapd:UI_Description xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Query results</ceapd:UI_Description>
                    <ceapd:UCD xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                    <ceapd:DefaultValue xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                    <ceapd:Units xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                </cea:ParameterDefinition>
                <cea:ParameterDefinition name="DEC" type="Dec">
                    <ceapd:UI_Name xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1">DEC</ceapd:UI_Name>
                    <ceapd:UI_Description xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Declination of cone</ceapd:UI_Description>
                    <ceapd:UCD xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                    <ceapd:DefaultValue xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                    <ceapd:Units xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                </cea:ParameterDefinition>
                <cea:ParameterDefinition name="Format" type="text">
                    <ceapd:UI_Name xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Format</ceapd:UI_Name>
                    <ceapd:UI_Description xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1">How the results are to be returned.  VOTABLE or CSV for now</ceapd:UI_Description>
                    <ceapd:UCD xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                    <ceapd:DefaultValue xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1">VOTABLE</ceapd:DefaultValue>
                    <ceapd:Units xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                </cea:ParameterDefinition>
                <cea:ParameterDefinition name="RA" type="RA">
                    <ceapd:UI_Name xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1">RA</ceapd:UI_Name>
                    <ceapd:UI_Description xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Right-Ascension of cone</ceapd:UI_Description>
                    <ceapd:UCD xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                    <ceapd:DefaultValue xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                    <ceapd:Units xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                </cea:ParameterDefinition>
                <cea:ParameterDefinition name="Radius" type="double">
                    <ceapd:UI_Name xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Radius</ceapd:UI_Name>
                    <ceapd:UI_Description xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Radius of cone</ceapd:UI_Description>
                    <ceapd:UCD xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                    <ceapd:DefaultValue xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                    <ceapd:Units xmlns:ceapd="http://www.astrogrid.org/schema/AGParameterDefinition/v1"/>
                </cea:ParameterDefinition>
            </cea:Parameters>
            <cea:Interfaces>
                <ceab:Interface name="cone" xmlns:ceab="http://www.astrogrid.org/schema/CommonExecutionArchitectureBase/v1">
                    <ceab:input>
                        <ceab:pref maxoccurs="1" minoccurs="1" ref="DEC"/>
                        <ceab:pref maxoccurs="1" minoccurs="1" ref="RA"/>
                        <ceab:pref maxoccurs="1" minoccurs="1" ref="Format"/>
                        <ceab:pref maxoccurs="1" minoccurs="1" ref="Radius"/>
                    </ceab:input>
                    <ceab:output>
                        <ceab:pref maxoccurs="1" minoccurs="1" ref="Result"/>
                    </ceab:output>
                </ceab:Interface>
                <ceab:Interface name="adql" xmlns:ceab="http://www.astrogrid.org/schema/CommonExecutionArchitectureBase/v1">
                    <ceab:input>
                        <ceab:pref maxoccurs="1" minoccurs="1" ref="Query"/>
                        <ceab:pref maxoccurs="1" minoccurs="1" ref="Format"/>
                    </ceab:input>
                    <ceab:output>
                        <ceab:pref maxoccurs="1" minoccurs="1" ref="Result"/>
                    </ceab:output>
                </ceab:Interface>
            </cea:Interfaces>
        </cea:ApplicationDefinition>
    </Resource>
</VODescription>