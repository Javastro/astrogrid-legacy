                    <ns2:UI_Description
                        xmlns:ns2="http://www.astrogrid.org/schema/AGParameterDefinition/v1">The
                        image that is used to detect sources. Basic
                        position,                 shape and size
                        informtaion is derived from this image</ns2:UI_Description>
                </cea:ParameterDefinition>
                <cea:ParameterDefinition name="PhotoImage" type="FileReference">
                    <ns3:UI_Name
                        xmlns:ns3="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Measurement Image</ns3:UI_Name>
                    <ns4:UI_Description
                        xmlns:ns4="http://www.astrogrid.org/schema/AGParameterDefinition/v1">The
                        Image that is used to measure photometric parameters</ns4:UI_Description>
                </cea:ParameterDefinition>
                <cea:ParameterDefinition name="config_file" type="FileReference">
                    <ns5:UI_Name
                        xmlns:ns5="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Configuration File</ns5:UI_Name>
                    <ns6:UI_Description
                        xmlns:ns6="http://www.astrogrid.org/schema/AGParameterDefinition/v1">File
                        containing the main configuration parameters for SExtractor</ns6:UI_Description>
                </cea:ParameterDefinition>
                <cea:ParameterDefinition name="PARAMETERS_NAME" type="FileReference">
                    <ns7:UI_Name
                        xmlns:ns7="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Parameter File</ns7:UI_Name>
                    <ns8:UI_Description
                        xmlns:ns8="http://www.astrogrid.org/schema/AGParameterDefinition/v1">A
                        File containing the list of catalogue paramters
                        that                 should be computed</ns8:UI_Description>
                </cea:ParameterDefinition>
                <cea:ParameterDefinition name="CATALOG_NAME" type="MySpace_FileReference">
                    <ns9:UI_Name
                        xmlns:ns9="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Derived
                        output catalogue</ns9:UI_Name>
                    <ns10:UI_Description
                        xmlns:ns10="http://www.astrogrid.org/schema/AGParameterDefinition/v1">The
                        catalogue of images derived from the input images</ns10:UI_Description>
                </cea:ParameterDefinition>
            </cea:Parameters>
            <cea:Interfaces>
                <ns11:Interface name="Simple" xmlns:ns11="http://www.astrogrid.org/schema/CommonExecutionArchitectureBase/v1">
                    <ns11:input>
                        <ns11:pref ref="DetectionImage"/>
