					<ceapd:UI_Name>Deblend thresholds</ceapd:UI_Name>
					<ceapd:UI_Description> Number of deblending 
						sub-thresholds</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>32</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
