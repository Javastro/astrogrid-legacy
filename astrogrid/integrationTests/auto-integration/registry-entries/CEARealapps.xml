		<vr:Summary>
			<vr:Description>This is part of the astrogrid CEA.</vr:Description>
			<vr:ReferenceURL>
				http://www.astrogrid.org/maven/build/applications/</vr:ReferenceURL>
		</vr:Summary>
		<vr:Type>Other</vr:Type>
		<vr:Curation>
			<vr:Publisher>
				<vr:Title>Astrogrid</vr:Title>
				<vr:Description>blah blab</vr:Description>
				<vr:ReferenceURL>http://www.astrogrid.org/</vr:ReferenceURL>
			</vr:Publisher>
			<vr:Contact>
				<vr:Name>Paul Harrison</vr:Name>
				<vr:Email>pah@jb.man.ac.uk</vr:Email>
			</vr:Contact>
			<vr:Date role="representative">2004-03-26</vr:Date>
			<vr:Creator>
				<vr:Name>Astrogrid</vr:Name>
				<vr:Logo>??</vr:Logo>
			</vr:Creator>
			<vr:Version>Iteration 5</vr:Version>
		</vr:Curation>
		<vr:Subject>???</vr:Subject>
		<cea:ApplicationDefinition>
			<cea:Parameters>
				<cea:ParameterDefinition name="PROBS_LITE" type="boolean">
					<ceapd:UI_Name>PROBS_LITE</ceapd:UI_Name>
					<ceapd:UI_Description>Save only the final probability 
						distribution</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>no</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="ZMIN" type="double">
					<ceapd:UI_Name>ZMIN</ceapd:UI_Name>
					<ceapd:UI_Description>minimum 
						redshift</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>0.01</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="MADAU" type="boolean">
					<ceapd:UI_Name>MADAU</ceapd:UI_Name>
					<ceapd:UI_Description>Apply Madau correction to 
						spectra</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>yes</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="GET_Z" type="boolean">
					<ceapd:UI_Name>GET_Z</ceapd:UI_Name>
					<ceapd:UI_Description>Actually obtain 
						photo-z</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>yes</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="DZ" type="double">
					<ceapd:UI_Name>DZ</ceapd:UI_Name>
					<ceapd:UI_Description>redshift 
						resolution</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>0.01</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="PROBS" type="boolean">
					<ceapd:UI_Name>PROBS</ceapd:UI_Name>
					<ceapd:UI_Description>Save all the galaxy probability 
						distributions (very large file 
						created)</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue/>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="MAG" type="boolean">
					<ceapd:UI_Name>MAG</ceapd:UI_Name>
					<ceapd:UI_Description>Data in 
						magnitudes?</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>yes</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="EXCLUDE" type="text">
					<ceapd:UI_Name>EXCLUDE</ceapd:UI_Name>
					<ceapd:UI_Description>Filters to be excluded from the 
						estimation</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>none</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="Z_THR" type="double">
					<ceapd:UI_Name>Z_THR</ceapd:UI_Name>
					<ceapd:UI_Description>Integrate likelihood for 
						z&amp;gt;z_thr</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>0</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="ZMAX" type="double">
					<ceapd:UI_Name>ZMAX</ceapd:UI_Name>
					<ceapd:UI_Description>maximum 
						redshift</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>6.5</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="PRIOR" type="text">
					<ceapd:UI_Name>PRIOR</ceapd:UI_Name>
					<ceapd:UI_Description>prior name</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>hdfn</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="CHECK" type="boolean">
					<ceapd:UI_Name>CHECK</ceapd:UI_Name>
					<ceapd:UI_Description>Perform some checks, compare observed 
						colors with templates, etc.</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>no</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="COLOR" type="boolean">
					<ceapd:UI_Name>COLOR</ceapd:UI_Name>
					<ceapd:UI_Description>Use colors instead of 
						fluxes</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>no</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="ONLY_TYPE" type="boolean">
					<ceapd:UI_Name>ONLY_TYPE</ceapd:UI_Name>
					<ceapd:UI_Description>Use spectroscopic redshifts instead 
						of photo-z</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>no</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="SPECTRA" type="text">
					<ceapd:UI_Name>Spectra</ceapd:UI_Name>
					<ceapd:UI_Description>template list</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>CWWSB.list</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="NEW_AB" type="boolean">
					<ceapd:UI_Name>NEW_AB</ceapd:UI_Name>
					<ceapd:UI_Description>If yes, generate new AB files even if 
						they already exist</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>no</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="ODDS" type="double">
					<ceapd:UI_Name>ODDS</ceapd:UI_Name>
					<ceapd:UI_Description>Odds threshold: affects confidence 
						limits definition</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>0.95</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="VERBOSE" type="boolean">
					<ceapd:UI_Name>VERBOSE</ceapd:UI_Name>
					<ceapd:UI_Description>Print estimated redshifts to the 
						standard output</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>yes</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="INTERP" type="integer">
					<ceapd:UI_Name>INTERP</ceapd:UI_Name>
					<ceapd:UI_Description>Number of interpolated templates 
						between each of the original ones</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>0</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="PLOTS" type="boolean">
					<ceapd:UI_Name>PLOTS</ceapd:UI_Name>
					<ceapd:UI_Description>produce plots</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>no</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
			</cea:Parameters>
			<cea:Interfaces>
				<ceab:Interface name="primary">
					<ceab:input>
						<ceab:pref ref="DZ" minoccurs="0" maxoccurs="1"/>
					</ceab:input>
					<ceab:output/>
				</ceab:Interface>
			</cea:Interfaces>
		</cea:ApplicationDefinition>
	</vr:Resource>
