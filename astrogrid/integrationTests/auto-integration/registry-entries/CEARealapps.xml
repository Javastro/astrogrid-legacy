					<ceapd:UI_Name>Cleaning efficiency</ceapd:UI_Name>
					<ceapd:UI_Description>Cleaning 
						Efficiency</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>1.0</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
