					<ceapd:UI_Name>neural net file</ceapd:UI_Name>
					<ceapd:UI_Description> Neural-Network_Weight table 
						filename</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue/>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="DetectionImage" type="binary">
					<ceapd:UI_Name>Detection Image</ceapd:UI_Name>
					<ceapd:UI_Description>The image that is used to detect 
						sources. Basic position, shape and size informtaion is 
						derived from this image</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue/>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="DEBLEND_MINCONT" type="double">
					<ceapd:UI_Name>de-blend contrast</ceapd:UI_Name>
					<ceapd:UI_Description> Minimum contrast parameter for 
						deblending</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>0.005</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="PhotoImage" type="binary">
					<ceapd:UI_Name>Measurement Image</ceapd:UI_Name>
					<ceapd:UI_Description>The Image that is used to measure 
						photometric parameters</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue/>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="CHECKIMAGE_TYPE" type="text">
					<ceapd:UI_Name>Check image</ceapd:UI_Name>
					<ceapd:UI_Description/>
					<ceapd:UCD/>
					<ceapd:DefaultValue/>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="ANALYSIS_THRESH" type="double">
					<ceapd:UI_Name>Analysis Threshold</ceapd:UI_Name>
					<ceapd:UI_Description>sigmas or threshold,ZP in 
						mag.arcsec-2</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>1.5</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
				<cea:ParameterDefinition name="PHOT_APERTURES" type="integer">
					<ceapd:UI_Name>Photometry apertures</ceapd:UI_Name>
					<ceapd:UI_Description> MAG_APER aperture diameter(s) in 
						pixels</ceapd:UI_Description>
					<ceapd:UCD/>
					<ceapd:DefaultValue>5</ceapd:DefaultValue>
					<ceapd:Units/>
				</cea:ParameterDefinition>
			</cea:Parameters>
			<cea:Interfaces>
				<ceab:Interface name="simple">
					<ceab:input>
						<ceab:pref ref="PARAMETERS_NAME" minoccurs="1" maxoccurs="1"/>
						<ceab:pref ref="config_file" minoccurs="1" maxoccurs="1"/>
						<ceab:pref ref="DetectionImage" minoccurs="1" maxoccurs="1"/>
						<ceab:pref ref="FILTER_NAME" minoccurs="1" maxoccurs="1"/>
						<ceab:pref ref="PhotoImage" minoccurs="1" maxoccurs="1"/>
						<ceab:pref ref="ANALYSIS_THRESH" minoccurs="0" maxoccurs="1"/>
					</ceab:input>
					<ceab:output>
						<ceab:pref ref="CATALOG_NAME" minoccurs="1" maxoccurs="1"/>
					</ceab:output>
				</ceab:Interface>
			</cea:Interfaces>
		</cea:ApplicationDefinition>
	</vr:Resource>
