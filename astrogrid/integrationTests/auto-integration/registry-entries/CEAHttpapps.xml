                <vr:Name>John Taylor</vr:Name>
                <vr:Email>jdt@roe.ac.uk</vr:Email>
            </vr:Contact>
            <vr:Date role="representative">2004-03-26</vr:Date>
            <vr:Creator>
                <vr:Name>Astrogrid</vr:Name>
                <vr:Logo>??</vr:Logo>
            </vr:Creator>
            <vr:Version>Iteration 6</vr:Version>
        </vr:Curation>
        <vr:Subject>???</vr:Subject>
        <cea:ApplicationDefinition>
			<cea:ApplicationKind>http</cea:ApplicationKind>
            <cea:Parameters>
                <cea:ParameterDefinition name="POS" type="text">
                    <ns1:UI_Name
                        xmlns:ns1="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Position</ns1:UI_Name>
                    <ns2:UI_Description
                        xmlns:ns2="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Comma separated position of centre of field RA,DEC in decimal degrees</ns2:UI_Description>
                    <ns4:Units xmlns:ns4="http://www.astrogrid.org/schema/AGParameterDefinition/v1">String</ns4:Units> <!--???? maybe degrees?-->
                </cea:ParameterDefinition>
                <cea:ParameterDefinition name="SIZE" type="text">
                    <ns1:UI_Name
                        xmlns:ns1="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Size</ns1:UI_Name>
                    <ns2:UI_Description
                        xmlns:ns2="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Comma separated size of field RA,DEC in decimal degrees</ns2:UI_Description>
                    <ns4:Units xmlns:ns4="http://www.astrogrid.org/schema/AGParameterDefinition/v1">String</ns4:Units>
                </cea:ParameterDefinition>
                <cea:ParameterDefinition name="FORMAT" type="text">
                    <ns1:UI_Name
                        xmlns:ns1="http://www.astrogrid.org/schema/AGParameterDefinition/v1">Format</ns1:UI_Name>
                    <ns2:UI_Description
                        xmlns:ns2="http://www.astrogrid.org/schema/AGParameterDefinition/v1">e.g. image/fits - see SIAP definition</ns2:UI_Description>
                    <ns4:Units xmlns:ns4="http://www.astrogrid.org/schema/AGParameterDefinition/v1">String</ns4:Units>
                </cea:ParameterDefinition>
                <cea:ParameterDefinition name="IMAGES" type="text"> <!--actually a VOTABLE-->
                    <ns1:UI_Name
                        xmlns:ns1="http://www.astrogrid.org/schema/AGParameterDefinition/v1">ImagesTable</ns1:UI_Name>
                    <ns2:UI_Description
                        xmlns:ns2="http://www.astrogrid.org/schema/AGParameterDefinition/v1">A VOTable of images</ns2:UI_Description>
                    <ns4:Units xmlns:ns4="http://www.astrogrid.org/schema/AGParameterDefinition/v1">String</ns4:Units>
                </cea:ParameterDefinition>												
            </cea:Parameters>
            <cea:Interfaces>
                <ns29:Interface name="Standard" xmlns:ns29="http://www.astrogrid.org/schema/CommonExecutionArchitectureBase/v1">
                    <ns29:input>
						<ns29:pref ref="POS"/>
						<ns29:pref ref="SIZE"/>
    					<ns29:pref ref="FORMAT"/>
					</ns29:input>
                    <ns29:output>
                        <ns29:pref ref="IMAGES"/>
                    </ns29:output>
                </ns29:Interface>
            </cea:Interfaces>
        </cea:ApplicationDefinition>
		<cea:CeaHttpAdapterSetup>
			<ceab:URL>http://archive.ast.cam.ac.uk/cgi-bin/wfs-siap/queryImage</ceab:URL>>
		</cea:CeaHttpAdapterSetup>
    </vr:Resource>
