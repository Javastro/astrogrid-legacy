<?xml version="1.0" encoding="UTF-8"?>
<v1:Select xmlns:v1="http://www.ivoa.net/xml/ADQL/v1.0">
    <v1:SelectionList>
        <v1:Item xsi:type="v1:allSelectionItemType" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/>
    </v1:SelectionList>
    <v1:From>
        <v1:Table Name="personnel" xsi:type="v1:tableType" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/>
    </v1:From>
    <v1:Where>
        <v1:Condition xsi:type="v1:inclusiveSearchType" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <v1:Expression Name="city_name" xsi:type="v1:columnReferenceType"/>
            <v1:Set xsi:type="v1:subQuerySet">
                <v1:selection>
                    <v1:SelectionList>
                        <v1:Item Name="city_name" xsi:type="v1:columnReferenceType"/>
                    </v1:SelectionList>
                    <v1:From>
                        <v1:Table Name="cities" xsi:type="v1:tableType"/>
                    </v1:From>
                    <v1:Where>
                        <v1:Condition Comparison="=" xsi:type="v1:comparisonPredType">
                            <v1:Arg Name="state" xsi:type="v1:columnReferenceType"/>
                            <v1:Arg xsi:type="v1:atomType">
                                <v1:Literal Value="NY" xsi:type="v1:stringType"/>
                            </v1:Arg>
                        </v1:Condition>
                    </v1:Where>
                </v1:selection>
            </v1:Set>
        </v1:Condition>
    </v1:Where>
</v1:Select>