<?xml version="1.0" encoding="UTF-8"?>
<v1:Select xmlns:v1="http://www.ivoa.net/xml/ADQL/v1.0">
    <v1:SelectionList>
        <v1:Item xsi:type="v1:allSelectionItemType" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/>
    </v1:SelectionList>
    <v1:From>
        <v1:Table Name="people" xsi:type="v1:tableType" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/>
    </v1:From>
    <v1:Where>
        <v1:Condition xsi:type="v1:likePredType" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <v1:Arg Name="name" xsi:type="v1:columnReferenceType"/>
            <v1:Pattern>
                <v1:Literal Value="%son" xsi:type="v1:stringType"/>
            </v1:Pattern>
        </v1:Condition>
    </v1:Where>
</v1:Select>