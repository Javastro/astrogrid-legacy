<?xml version="1.0" encoding="UTF-8"?>
<!--+
    | Maven project description for the AstroGrid registry components.
    +-->
<project
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="maven-project.xsd">
   <extend>${basedir}/../../maven-base/project.xml</extend>

    <currentVersion>${astrogrid.eXist.version}</currentVersion>

    <!--+
        | Describes the licenses for this project.
    <licenses>
    </licenses>
        +-->

    <!--+
        | The external libraries required to build our components.
        +-->
    <dependencies>
        <!--+
            | The Axis webapp war file.
            +-->
        <dependency>
            <groupId>axis</groupId>
            <artifactId>axis</artifactId>
            <version>1.1</version>
            <type>war</type>
            <url>http://ws.apache.org/axis/index.html</url>
        </dependency>
        <!--+
            | The 3rd party jars that Axis depends on.

        <dependency>
            <groupId>jaf</groupId>
            <artifactId>jaf</artifactId>
            <version>1.0.2</version>
            <type>jar</type>
        </dependency>
            +-->        
        <!--+
            | The JConfig jars.
            | Soon to be replaced by the AstroGrid common tools.

        <dependency>
            <groupId>jconfig</groupId>
            <artifactId>jconfig</artifactId>
            <version>2.2</version>
            <type>jar</type>
            <url>http://www.jconfig.org/</url>
            <properties>
                <dist.zip>true</dist.zip>
            </properties>
        </dependency>
                    +-->
    </dependencies>


</project>
