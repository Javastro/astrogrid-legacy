							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="abortResponse">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="abortReturn" type="xsd:boolean">
							<xsd:annotation>
								<xsd:documentation>returns true if the application was successfully aborted</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
