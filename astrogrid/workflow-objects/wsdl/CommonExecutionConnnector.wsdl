								<xsd:documentation>The identifier for the job step being executed</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="jobMonitorURL" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>The endpoint of the JobMonitor service that is to be used for status callbacks</xsd:documentation>
