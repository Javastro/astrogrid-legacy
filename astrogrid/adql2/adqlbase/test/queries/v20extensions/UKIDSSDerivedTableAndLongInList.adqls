SELECT 
	las.ra as alpha, las.dec as delta, ymj_1Pnt, j_1mhPnt, hmkPnt, yAperMag3-kAperMag3 as ymk, 
	j_1AperMag3-kAperMag3 as jmk, yaperMag3, j_1aperMag3, haperMag3, kaperMag3, j_m, h_m, k_m, 
	3.6e3*cos(radians(las.dec))*(las.ra-m2.ra)/((mj.mjdObs - jdate+2400000.5)/365.25) AS pmRA, 
	3.6e3*(las.dec-m2.dec)/((mj.mjdObs - jdate+2400000.5)/365.25) AS pmDec, 
	sqrt(1.296e7*(power(cos(radians(las.dec))*(las.ra-m2.ra)/((mj.mjdObs - jdate+2400000.5)/365.25),2) + power((las.dec-m2.dec)/((mj.mjdObs - jdate+2400000.5)/365.25),2))) as totalPM, 
	5.0+las.j_1AperMag3 + 5.0*log10(sqrt(1.296e7*(power(cos(radians(las.dec))*(las.ra-m2.ra)/((mj.mjdObs - jdate+2400000.5)/365.25),2) + power((las.dec-m2.dec)/((mj.mjdObs - jdate+2400000.5)/365.25),2)))) as HJ, 
	T1.num2MASSNeighbs 
FROM lasSource AS las, 
     TWOMASS..twomass_psc AS m2, 
     lasSourceXtwomass_psc AS x, 
     lasMergeLog AS l, 
     Multiframe AS mj, 
     /* Get the counts of 2MASS neighbours to aid identification of possible spurious associations: */ 
     (SELECT masterObjID, count(*) AS num2MASSNeighbs FROM lasSourceXtwomass_psc GROUP BY masterObjID) AS T1 
WHERE 
	/* Joining predicates: */ 
	las.frameSetID=l.frameSetID AND 
	j_1mfID=mj.multiframeID AND 
	x.masterObjID=las.sourceID AND 
	slaveObjID=m2.pts_key AND 
	T1.masterObjID=x.masterObjID 
	/* Choose the nearest 2MASS neighbour only in each case: */ 
	AND distanceMins IN (SELECT MIN(distanceMins) FROM lasSourceXtwomass_psc WHERE masterObjID=x.masterObjID) 
	/* Select only good 2MASS detections (astrometric errors blow up fainter than this): */ 
	AND m2.j_m<15.8 
