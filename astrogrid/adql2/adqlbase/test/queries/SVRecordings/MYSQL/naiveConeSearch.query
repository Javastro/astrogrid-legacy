+=====================================================+
|  Input ADQL:                                        |
+=====================================================+
SELECT * FROM RASS_PHOTONS as r
WHERE 2 * ASIN( SQRT( SIN( (20-r."DEC")/2 ) 
   * SIN((20-r."DEC")/2) + COS(20) * COS(r."DEC") 
   * SIN((20 - r.RA)/2) * SIN((20 - r.RA)/2 ) ) ) <= 10 ;
+=====================================================+
|  Output SQL:                                        |
+=====================================================+
Select *  From RASS_PHOTONS as r  Where (2 * ASIN(SQRT(((SIN((((20 - r.DEC) )  / 2) )  * SIN((((20 - r.DEC) )  / 2) ) )  + (COS(20)  * (COS(r.DEC)  * (SIN((((20 - r.RA) )  / 2) )  * SIN((((20 - r.RA) )  / 2) ) ) ) ) ) ) ) )  <= 10 