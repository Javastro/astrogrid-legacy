<project>
    <extend>${basedir}/../../../maven-base/project.xml</extend>
    <currentVersion>${astrogrid.aggregate.version}</currentVersion>
    <id>aladin-avo</id>
    <name>CDS Aladin (AVO Demonstration)</name>
    <description>Package of Aladin with Myspace Plugin for the AVO</description>
    <shortDescription>Aladin Interactive Software Sky Atlas</shortDescription>
    <dependencies>

        <dependency>
                <groupId>cds</groupId>
                <artifactId>AVO</artifactId>
                <version>2.002</version>
                <properties>
                    <jnlp.jar>true</jnlp.jar>
                    <jnlp.main.jar>true</jnlp.main.jar>
                </properties>
        </dependency>

        <!-- stuff required in maven build -->
        <dependency>
            <groupId>commons-jelly</groupId>
            <artifactId>commons-jelly-tags-jsl</artifactId>
            <version>20030211.143151</version>
            <properties/>
        </dependency>
        <dependency>
            <groupId>commons-jelly</groupId>
            <artifactId>commons-jelly-tags-xml</artifactId>
            <version>20030211.142705</version>
            <url>http://jakarta.apache.org/commons/jelly/libs/xml/</url>
            <properties/>
        </dependency>
  </dependencies>
    <build>

        <sourceDirectory/>

        <unitTestSourceDirectory/>

        <unitTest/>
    </build>

</project>
