<?xml version="1.0" encoding="UTF-8"?>
<project>
  <extend>${basedir}/../../../maven-base/project.xml</extend>

  <currentVersion>${astrogrid.aggregate.version}</currentVersion>

  <id>soap</id>

  <name>Axis SOAP Libraries</name>

  <description>Axis SOAP and it's prerequisites bundled as a JNLP Extension</description>

  <shortDescription>Axis Libraries</shortDescription>

  <dependencies>

<!-- 3rd party libs -->
    <dependency>
      <groupId>xerces</groupId>

      <artifactId>xerces</artifactId>

      <version>2.4.0-patched</version>

      <type>jar</type>

      <url>http://xml.apache.org/</url>

      <properties>
        <jnlp.jar>true</jnlp.jar>
      </properties>
    </dependency>

    <dependency>
      <groupId>xalan</groupId>

      <artifactId>xalan</artifactId>

      <version>2.4.1</version>

      <type>jar</type>

      <url>http://xml.apache.org/</url>

      <properties>
        <jnlp.jar>true</jnlp.jar>
      </properties>
    </dependency>

    <dependency>
      <groupId>xml-apis</groupId>

      <artifactId>xml-apis</artifactId>

      <version>1.0.b2-patched</version>

      <type>jar</type>

      <url>http://xml.apache.org/</url>

      <properties>
        <jnlp.jar>true</jnlp.jar>
      </properties>
    </dependency>

    <dependency>
      <artifactId>axis</artifactId>

      <groupId>axis</groupId>

      <version>1.1</version>

      <url>http://ws.apache.org/axis/</url>

      <properties>
        <jnlp.jar>true</jnlp.jar>
      </properties>
    </dependency>

    <dependency>
      <artifactId>jax-qname</artifactId>

      <groupId>jax-qname</groupId>

      <version>UNKNOWN</version>

      <jar>jax-qname.jar</jar>

      <url>http://ws.apache.org/axis/</url>

      <properties>
        <jnlp.jar>true</jnlp.jar>
      </properties>
    </dependency>

    <dependency>
      <artifactId>jaxrpc-api</artifactId>

      <groupId>jaxrpc-api</groupId>

      <version>1.0.3</version>

      <url>http://ws.apache.org/axis/</url>

      <properties>
        <jnlp.jar>true</jnlp.jar>
      </properties>
    </dependency>

    <dependency>
      <artifactId>saaj-api</artifactId>

      <groupId>saaj-api</groupId>

      <version>1.1.1</version>

      <url>http://ws.apache.org/axis/</url>

      <properties>
        <jnlp.jar>true</jnlp.jar>
      </properties>
    </dependency>

    <dependency>
      <artifactId>wsdl4j</artifactId>

      <groupId>wsdl4j</groupId>

      <jar>wsdl4j.jar</jar>

      <url>http://ws.apache.org/axis/</url>

      <properties>
        <jnlp.jar>true</jnlp.jar>
      </properties>
    </dependency>

    <dependency>
      <artifactId>commons-discovery</artifactId>

      <groupId>commons-discovery</groupId>

      <version>20030211.213356fixed1</version>

      <url>http://jakarta.apache.org/commons/</url>

      <properties>
        <jnlp.jar>true</jnlp.jar>
      </properties>
    </dependency>

    <dependency>
      <artifactId>commons-logging</artifactId>

      <groupId>commons-logging</groupId>

      <version>1.0.3</version>

      <url>http://jakarta.apache.org/commons/</url>

      <properties>
        <jnlp.jar>true</jnlp.jar>
      </properties>
    </dependency>

    <dependency>
      <id>log4j</id>

      <version>1.2.8</version>

      <properties>
        <jnlp.jar>true</jnlp.jar>

      </properties>
    </dependency>

    <dependency>
      <artifactId>commons-collections</artifactId>

      <groupId>commons-collections</groupId>

      <version>3.1</version>

      <url>http://jakarta.apache.org/commons/</url>

      <properties>
        <jnlp.jar>true</jnlp.jar>

      </properties>
    </dependency>

<!-- stuff required in maven build -->
    <dependency>
      <groupId>commons-jelly</groupId>

      <artifactId>commons-jelly-tags-jsl</artifactId>

      <version>20030211.143151</version>

      <properties />
    </dependency>

    <dependency>
      <groupId>commons-jelly</groupId>

      <artifactId>commons-jelly-tags-xml</artifactId>

      <version>20030211.142705</version>

      <url>http://jakarta.apache.org/commons/jelly/libs/xml/</url>

      <properties />
    </dependency>
  </dependencies>

  <build>
    <sourceDirectory />

    <unitTestSourceDirectory />

    <unitTest />
  </build>
</project>

