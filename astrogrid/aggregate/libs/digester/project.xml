<?xml version="1.0" encoding="UTF-8"?>
<project>
  <extend>${basedir}/../../../maven-base/project.xml</extend>

  <currentVersion>${astrogrid.aggregate.version}</currentVersion>

  <id>digester</id>

  <name>Commons XML Digester</name>

  <description>Jakarta Digester and it's prerequisites bundled as a JNLP Extension</description>

  <shortDescription>Jakarta Commons Digester</shortDescription>

  <dependencies>
	  
	  <dependency>
		  <artifactId>commons-beanutils</artifactId>
		  <groupId>commons-beanutils</groupId>
		  <version>1.6</version>
		  <type>jar</type>
		  <properties>
			  <jnlp.jar>true</jnlp.jar>
		  </properties>
	  </dependency>
	  <!-- not needed for latest version -->
	  <dependency>
		  <artifactId>commons-digester</artifactId>
		  <groupId>commons-digester</groupId>
		  <version>1.6</version>
		  <type>jar</type>
		  <properties>
			  <jnlp.jar>true</jnlp.jar>
		  </properties>		  
 </dependency>	
 
 <dependency>
	 <artifactId>commons-lang</artifactId>
	 <groupId>commons-lang</groupId>
	 <version>2.0</version>
	 <type>jar</type>
	 <properties>
		 <jnlp.jar>true</jnlp.jar>
	 </properties>
        </dependency>

<!-- stuff required in maven build -->
    <dependency>
      <groupId>commons-jelly</groupId>

      <artifactId>commons-jelly-tags-jsl</artifactId>

      <version>20030211.143151</version>

      <properties />
    </dependency>

    <dependency>
      <groupId>commons-jelly</groupId>

      <artifactId>commons-jelly-tags-xml</artifactId>

      <version>20030211.142705</version>

      <url>http://jakarta.apache.org/commons/jelly/libs/xml/</url>

      <properties />
    </dependency>
  </dependencies>

  <build>
    <sourceDirectory />

    <unitTestSourceDirectory />

    <unitTest />
  </build>
</project>

