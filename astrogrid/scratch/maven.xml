<?xml version="1.0"?>
<!-- extension maven script -->
<project xmlns:maven="jelly:maven">
    <goal name="usereactor">
		 <maven:reactor
		     basedir="${basedir}"
		     postProcessing="false"
		     includes="sub/project.xml"
		     goals="showprops"
		     banner="running subprojects"
		     ignoreFailures="true" />
	</goal>
</project>
