<community version="1.0">

	<permissions>
		<permission group="amphibians" resource="portal.site" action="read">
			<status>GRANT</status>
			<reason>Amphibians are ok</reason>
		</permission>
		<permission group="amphibians" resource="portal.admin" action="read">
			<status>GRANT</status>
			<reason>Amphibians are trusted</reason>
		</permission>

		<permission group="reptiles" resource="portal.site" action="read">
			<status>GRANT</status>
			<reason>Reptiles are ok</reason>
		</permission>
		<permission group="reptiles" resource="portal.admin" action="read">
			<status>REVOKE</status>
			<reason>Reptiles are not trusted</reason>
		</permission>

	</permissions>

</community>
