<?xml version="1.0" encoding="UTF-8"?><project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.astrogrid</groupId>
  <artifactId>astrogrid-community</artifactId>
  <packaging>war</packaging>
  <name>Community Web Application</name>
  <version>2008.2</version>

  <repositories>
    <repository>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
      <id>ag-m1-repository</id>
      <name>AstroGrid's Maven-1 repository</name>
      <url>http://www.astrogrid.org/maven</url>
      <layout>legacy</layout>
    </repository>
  </repositories>

  <description>Community is a AstroGrid web-application to manage user accounts.</description>
  <inceptionYear>2003</inceptionYear>
  <developers>
    <developer>
      <id>dave</id>
      <name>Dave Morris</name>
      <organization>IoA, Cambridge</organization>
      <roles>
        <role>Original developer</role>
      </roles>
      <timezone>0</timezone>
    </developer>
    <developer>
      <id>gtr</id>
      <name>Guy Rixon</name>
      <organization>IoA Cambridge</organization>
      <roles>
        <role>maintainer</role>
      </roles>
      <timezone>0</timezone>
    </developer>
  </developers>
  <contributors>
    <contributor>
      <name>Gary Gilchrist</name>
      <organization>University of Leicester</organization>
      <roles>
        <role>Tester</role>
      </roles>
      <timezone>0</timezone>
    </contributor>
    <contributor>
      <name>Mark Holliman</name>
      <organization>WFAU, Edinburgh</organization>
      <roles>
        <role>Tester</role>
      </roles>
      <timezone>0</timezone>
    </contributor>
  </contributors>
  <scm>
    <connection>scm:cvs:pserver:anoncvs@cvs.astrogrid.org:/devel:astrogrid/community/webapp</connection>
    <developerConnection>scm:cvs:ext:${maven.username}@cvs.astrogrid.org:/devel:astrogrid/community/webapp</developerConnection>
  </scm>
  <organization>
    <name>AstroGrid</name>
    <url>http://www.astrogrid.org</url>
  </organization>
  <build>
    <sourceDirectory>src/java</sourceDirectory>
    <testSourceDirectory>src/junit</testSourceDirectory>
    <resources>
      <resource>
        <directory>src/config/database</directory>
        <includes>
          <include>*.xml</include>
          <include>*.sql</include>
        </includes>
      </resource>
    </resources>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <configuration>
          <includes>
            <include>**/*Test.java</include>
          </includes>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
