      <version>2008.0</version>
    </dependency>
    <dependency>
      <groupId>org.astrogrid</groupId>
      <artifactId>astrogrid-registry-common</artifactId>
      <version>2008.0.04</version>
    </dependency>
    <dependency>
      <groupId>org.astrogrid</groupId>
