<?xml version='1.0'?>
<handler-registry>
  <!-- Here and below, the IDs aren't of huge significance, but
       are only mnemonic.  The prefixes are there to try to avoid collisions
       with any prefix declared in converted XSchema files. -->
  <xml-ns id='x-cone'
          >http://www.ivoa.net/xml/ConeSearch/v1.0</xml-ns>
  <xml-ns id='x-ds05'
          >http://www.ivoa.net/xml/VODataService/v0.5</xml-ns>
  <xml-ns id='x-ds'
          >http://www.ivoa.net/xml/VODataService/v1.0</xml-ns>
  <xml-ns id='x-ri01'
          >http://www.ivoa.net/xml/RegistryInterface/v0.1</xml-ns>
  <xml-ns id='x-ri'
          >http://www.ivoa.net/xml/RegistryInterface/v1.0</xml-ns>
  <xml-ns id='x-reg03'
          >http://www.ivoa.net/xml/VORegistry/v0.3</xml-ns>
  <xml-ns id='x-reg'
          >http://www.ivoa.net/xml/VORegistry/v1.0</xml-ns>
  <xml-ns id='x-res010'
          >http://www.ivoa.net/xml/VOResource/v0.10</xml-ns>
  <xml-ns id='x-res'
          >http://www.ivoa.net/xml/VOResource/v1.0</xml-ns>
  <xml-ns id='x-rsr'
          >urn:astrogrid:schema:RegistryStoreResource:v1</xml-ns>
  <xml-ns id='x-sia07'
          >http://www.ivoa.net/xml/SIA/v0.7</xml-ns>
  <xml-ns id='x-sia'
          >http://www.ivoa.net/xml/SIA/v1.0</xml-ns>
  <xml-ns id='x-ssa'
          >http://www.ivoa.net/xml/SSA/v0.4</xml-ns>
  <xml-ns id='x-stc130'
          >http://www.ivoa.net/xml/STC/stc-v1.30.xsd</xml-ns>
  <xml-ns id='x-tdb03'
          >urn:astrogrid:schema:vo-resource-types:TabularDB:v0.3</xml-ns>
  <xml-ns id='x-sr'
          >urn:astrogrid:schema:StoreResources:v1</xml-ns>

  <!-- The following namespaces should include the trailing '#'.
       This isn't arbitrary, but must match the namespace names in the
       RDFS ontologies generated by ../xschemas/xschema2rdfs.lx, and the
       XSLT scripts generated by xschema2xslt.lx -->
  <ontology-ns id='o-cone'
               >http://www.ivoa.net/xml/ConeSearch/v1.0#</ontology-ns>
  <ontology-ns id='o-ds'
               >http://www.ivoa.net/xml/VODataService/v1.0#</ontology-ns>
  <ontology-ns id='o-reg'
               >http://www.ivoa.net/xml/VORegistry/v1.0#</ontology-ns>
  <ontology-ns id='o-res'
               >http://www.ivoa.net/xml/VOResource/v1.0#</ontology-ns>
  <ontology-ns id='o-sia'
               >http://www.ivoa.net/xml/SIA/v1.0#</ontology-ns>
  <ontology-ns id='o-ssa'
               >http://www.ivoa.net/xml/SSA/v0.4#</ontology-ns>
  <ontology-ns id='o-stc130'
               >http://www.ivoa.net/xml/STC/stc-v1.30.xsd#</ontology-ns>
  <ontology-ns id='o-tdb'
               >urn:astrogrid:schema:vo-resource-types:TabularDB:v0.3#</ontology-ns>

  <!-- Here's the list of handlers.  The <xslt> element names the .xslt
       script which will convert between the listed namespaces.
       There should be only a single script which converts to a given
       target namespace (we can't just list everything, because the
       elementFormDefault=unqualified in the schemas means we'd get
       namespace collisions, grrr).  Scripts which are not to be
       included should have an attribute status=disabled in their
       <handler> element.
  -->
  <handler from='x-res010' to='o-res' status='disabled'>
    <xslt>rdf-VOResource-0.10.xslt</xslt>
  </handler>
  <handler from='x-res' to='o-res'>
    <xslt>rdf-VOResource-1.0.xslt</xslt>
  </handler>
  <handler from='x-ri01' to='o-res' status='disabled'>
    <xslt>rdf-RegistryInterface-0.1.xslt</xslt>
  </handler>
  <handler from='x-ri' to='o-res'>
    <xslt>rdf-RegistryInterface-1.0.xslt</xslt>
  </handler>
  <handler from='x-cone' to='o-cone'>
    <xslt>ConeSearch-v1.0.xslt</xslt>                 <!--generated-->
  </handler>
  <handler from='x-sia07' to='o-sia' status='disabled'>
    <xslt>rdf-SIA-0.7.xslt</xslt>
  </handler>
  <handler from='x-sia' to='o-sia'>
    <xslt>SIA-v1.0.xslt</xslt>                        <!--generated-->
  </handler>
  <handler from='x-ssa' to='o-ssa'>
    <xslt>SSA-v0.4.xslt</xslt>                        <!--generated-->
  </handler>
  <handler from='x-stc130' to='o-stc130'>
    <xslt>stc-v1.30.xslt</xslt>                       <!--generated-->
  </handler>
  <handler from='x-tdb03' to='o-tdb'>
    <xslt>TabularDB-v0.3.xslt</xslt>                  <!--generated-->
  </handler>
  <handler from='x-ds' to='o-ds'>
    <xslt>VODataService-v1.0.xslt</xslt>              <!--generated-->
  </handler>
  <handler from='x-ds05' to='o-ds' status='disabled'>
    <xslt>rdf-VODataService-v0.5.xslt</xslt>
  </handler>
  <handler from='x-reg03' to='o-reg'>
    <xslt>VORegistry-v0.3.xslt</xslt>                 <!--generated-->
  </handler>
  <handler from='x-reg' to='o-reg'>
    <xslt>VORegistry-v1.0.xslt</xslt>                 <!--generated-->
  </handler>


  <!-- null handlers: ignore these namespaces -->
  <handler from='x-rsr'/>
  <handler from='x-sr'/>

  <!-- Support for the test harness.
       See the XSchema ../xschemas/test-schema.xsd. -->
  <xml-ns id='x-test'
          >http://example.org/test</xml-ns>
  <ontology-ns id='o-test'
               >http://example.org/test#</ontology-ns>
  <handler from='x-test' to='o-test'>
    <xslt>test-schema.xslt</xslt>
  </handler>

</handler-registry>
